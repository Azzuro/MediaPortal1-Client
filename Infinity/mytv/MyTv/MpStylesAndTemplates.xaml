  <LinearGradientBrush x:Key="fadeOpacityBrush" EndPoint="1,1" StartPoint="0,0">
    <GradientStop Color="#FF000000" Offset="0"/>
    <GradientStop Color="#91000000" Offset="0.6"/>
    <GradientStop Color="#00000000" Offset="0.8"/>
  </LinearGradientBrush>
  <RadialGradientBrush x:Key="backGroundBrush" GradientOrigin="0.186,0.311">
    <RadialGradientBrush.RelativeTransform>
      <TransformGroup>
        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="1.071" ScaleY="1.29"/>
        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
        <TranslateTransform X="-0.036" Y="-0.146"/>
      </TransformGroup>
    </RadialGradientBrush.RelativeTransform>
    <GradientStop Color="#FF5C9AE6" Offset="0"/>
    <GradientStop Color="#FF1253A3" Offset="1"/>
  </RadialGradientBrush>
  <RadialGradientBrush x:Key="dialogBackGroundBrush" GradientOrigin="0.186,0.311">
    <RadialGradientBrush.RelativeTransform>
      <TransformGroup>
        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="1.071" ScaleY="1.29"/>
        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
        <TranslateTransform X="-0.036" Y="-0.146"/>
      </TransformGroup>
    </RadialGradientBrush.RelativeTransform>
    <GradientStop Color="#FF5BB0FF" Offset="0"/>
    <GradientStop Color="#FF238DFF" Offset="1"/>
  </RadialGradientBrush>
  <RadialGradientBrush x:Key="MpButtonBrush" GradientOrigin="0.85,0.82">
    <RadialGradientBrush.RelativeTransform>
      <TransformGroup>
        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="1.335" ScaleY="1.582"/>
        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
        <TranslateTransform X="0.027" Y="-0.006"/>
      </TransformGroup>
    </RadialGradientBrush.RelativeTransform>
    <GradientStop Color="#FF2A5EB4" Offset="0"/>
    <GradientStop Color="#FF2A5EB4" Offset="1"/>
  </RadialGradientBrush>
  <RadialGradientBrush  x:Key="MpButtonBrushLight" GradientOrigin="0.85,0.82">
    <RadialGradientBrush.RelativeTransform>
      <TransformGroup>
        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="1.335" ScaleY="1.582"/>
        <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
        <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
