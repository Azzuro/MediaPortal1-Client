<Plugin name        = "ImporterManager"
        author      = "Frodo"
        copyright   = "GPL"
        description = "Importer Manager">

  <Manifest>
    <Identity name = "MediaPortal.Plugins.Services.Importers.ImporterManager"/>
  </Manifest>

  <Runtime>
    <Import assembly = "ImporterManager.dll">
      <Builder name = 'Importer' class = 'MediaPortal.Plugins.Services.Importers.ImporterBuilder'/>
    </Import>
  </Runtime>

  <Register location = "/Services">
    <Class id = "IImporterManager" class = "MediaPortal.Plugins.Services.Importers.ImporterManager"/>
  </Register>
</Plugin>