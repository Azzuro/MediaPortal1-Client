<Plugin name        = "MovieImporter"
        author      = "Frodo"
        copyright   = "GPL"
        description = "Movie Importer">

  <Manifest>
    <Identity name = "MovieImporter.Importer"/>
  </Manifest>

  <Runtime>
    <Import assembly = "MovieImporter.dll"/>
  </Runtime>

  <Register location = "/Media/Importers">
    <Importer id = "MovieImporter" class = "MovieImporter.Importer" extensions=".wmv,.mpg,.avi,.mkv,.ts,.ifo"/>
  </Register>
</Plugin>