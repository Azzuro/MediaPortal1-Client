<Plugin name        = "PictureImporter"
        author      = "Frodo"
        copyright   = "GPL"
        description = "Picture Importer">

  <Manifest>
    <Identity name = "PictureImporter.Importer"/>
  </Manifest>

  <Runtime>
    <Import assembly = "PictureImporter.dll"/>
  </Runtime>

  <Register location = "/Media/Importers">
    <Importer id = "PictureImporter" class = "PictureImporter.Importer" extensions=".jpg,.png,.gif,.tga"/>
  </Register>
</Plugin>