<Plugin name        = "Movies"
        author      = "Team MediaPortal"
        copyright   = "GPL"
        description = "Movies Plugin">

  <Manifest>
    <Identity name = "Movies"/>
  </Manifest>

  <Runtime>
    <Import assembly = "MyMovies.dll"/>
  </Runtime>

  <Register location = "/Models">
    <Class id = "MyMovies" class = "MyMovies.Model"/>
  </Register>
</Plugin>