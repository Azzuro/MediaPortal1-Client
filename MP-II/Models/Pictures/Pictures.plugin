<Plugin name        = "Pictures"
        author      = "Team MediaPortal"
        copyright   = "GPL"
        description = "Pictures Plugin">

  <Manifest>
    <Identity name = "Models.Pictures.PictureModel"/>
  </Manifest>

  <Runtime>
    <Import assembly = "Pictures.dll"/>
  </Runtime>

  <Register location = "/Models/mypicture">
    <Class id = "PictureModel" class = "Models.Pictures.PictureModel"/>
  </Register>
</Plugin>