<Plugin name        = "Weather"
        author      = "Team MediaPortal"
        copyright   = "GPL"
        description = "Weather Plugin">

  <Manifest>
    <Identity name = "Models.Weather.WeatherViewModel"/>
  </Manifest>

  <Runtime>
    <Import assembly = "Weather.dll"/>
  </Runtime>

  <Register location = "/Models/Weather">
    <Class id = "MyWeather" class = "Models.Weather.WeatherViewModel"/>
  </Register>
</Plugin>