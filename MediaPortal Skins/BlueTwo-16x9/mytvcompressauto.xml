<window>
	<id>611</id>
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<define>#header.label:895</define>
	<define>#header.text:television</define>
	<define>#header.hover:hover_my tv.png</define>
	<define>#selecteditem:-</define>
	<controls>
		<import>common.window.default.xml</import>		
		<control>
			<description>Enable auto compress</description>
			<type>checkmark</type>
			<id>2</id>
			<posX>450</posX>
			<posY>180</posY>
			<width>300</width>
			<height>28</height>
			<align>left</align>
			<label>994</label>
			<textcolor>FFFFFFFF</textcolor>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>17</onup>
			<ondown>5</ondown>
		</control>
		<control>
			<description>Auto compress label</description>
			<type>label</type>
			<id>4</id>
			<posX>450</posX>
			<posY>220</posY>
			<label>895</label>
			<font>font14</font>
			<align>left</align>
			<textcolor>FFFFFFFF</textcolor>
		</control>
		<control>
			<description>Auto compress select</description>
			<type>spincontrol</type>
			<id>5</id>
			<posX>750</posX>
			<posY>226</posY>
			<reverse>yes</reverse>
			<onup>2</onup>
			<ondown>7</ondown>
			<onleft>5</onleft>
			<onright>5</onright>
			<align>right</align>
			<textcolor>FFFFFFFF</textcolor>
			<showrange>no</showrange>
			<spintype>text</spintype>
			<subitems>
				<subitem>1 Hour</subitem>
				<subitem>2 Hour</subitem>
				<subitem>3 Hour</subitem>
				<subitem>4 Hour</subitem>
				<subitem>5 Hour</subitem>
				<subitem>6 Hour</subitem>
				<subitem>7 Hour</subitem>
				<subitem>8 Hour</subitem>
			</subitems>
			<textureUp>arrow_round_right_nofocus.png</textureUp>
			<textureDown>arrow_round_left_nofocus.png</textureDown>
			<textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
			<textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
		</control>
		<control>
			<description>Delete original file</description>
			<type>checkmark</type>
			<id>7</id>
			<posX>450</posX>
			<posY>260</posY>
			<width>300</width>
			<height>28</height>
			<align>left</align>
			<label>992</label>
			<textcolor>FFFFFFFF</textcolor>
			<onleft>15</onleft>
			<onright>15</onright>
			<onup>5</onup>
			<ondown>99</ondown>
		</control>
	<import>common.mytv.preview.xml</import>
	</controls>
</window>