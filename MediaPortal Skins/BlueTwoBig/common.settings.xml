<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
    </control>
    <control>
      <description>Header Logo</description>
      <type>image</type>
      <id>1</id>
      <posX>60</posX>
      <posY>20</posY>
      <texture>#header.image</texture>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
    <control>
      <description>Header Label</description>
      <type>label</type>
      <id>1</id>
      <posX>32</posX>
      <posY>96</posY>
      <label>#header.label</label>
      <font>font18</font>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
    <control>
      <description>Hover image</description>
      <type>image</type>
      <id>1</id>
      <posX>77</posX>
      <posY>400</posY>
      <width>161</width>
      <height>158</height>
      <texture>#header.hover</texture>
      <visible>!videoplayer.usingoverlays</visible>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
  </controls>
</window>