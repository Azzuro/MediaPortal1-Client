<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>5002</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:859</define>
  <define>#header.image:alarm_logo.png</define>
  <define>#header.hover:hover_my alarm.png</define>
  <controls>
    <import>common.window.xml</import>
    <control>
      <description>sleep time label</description>
      <type>label</type>
      <id>1</id>
      <posX>220</posX>
      <posY>220</posY>
      <label>#currentsleeptime</label>
      <align>left</align>
      <font>font72</font>
      <textcolor>20ffffff</textcolor>
    </control>
    <control>
      <description>SleepTimer On/Off button</description>
      <type>togglebutton</type>
      <id>3</id>
      <posX>60</posX>
      <posY>97</posY>
      <font>font13</font>
      <textcolor>ffffffff</textcolor>
      <colordiffuse>ffffffff</colordiffuse>
      <label>856</label>
      <onright>4</onright>
      <onup>17</onup>
      <ondown>7</ondown>
    </control>
    <control>
      <description>Reset Button</description>
      <type>button</type>
      <id>7</id>
      <posX>60</posX>
      <posY>131</posY>
      <label>868</label>
      <onright>4</onright>
      <onup>3</onup>
      <ondown>3</ondown>
    </control>
    <control>
      <description>label1</description>
      <type>label</type>
      <id>1</id>
      <posX>270</posX>
      <posY>97</posY>
      <label>865</label>
      <font>font13</font>
      <align>left</align>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>alarm time minutes</description>
      <type>spincontrol</type>
      <spintype>int</spintype>
      <orientation>vertical</orientation>
      <digits>2</digits>
      <id>4</id>
      <posX>500</posX>
      <posY>100</posY>
      <showrange>no</showrange>
      <onup>6</onup>
      <ondown>5</ondown>
      <onleft>3</onleft>
      <onright>5</onright>
      <textcolor>FFFFFFFF</textcolor>
      <textureUp>arrow_round_up_nofocus.png</textureUp>
      <textureDown>arrow_round_down_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
    </control>
    <control>
      <description>fade out volume</description>
      <type>checkmark</type>
      <id>5</id>
      <posX>270</posX>
      <posY>131</posY>
      <width>200</width>
      <height>20</height>
      <label>866</label>
      <onup>4</onup>
      <onleft>3</onleft>
      <onright>6</onright>
      <textureFocus>check-box.png</textureFocus>
      <textureNoFocus>check-boxNF.png</textureNoFocus>
    </control>
    <control>
      <description>shutdown pc</description>
      <type>checkmark</type>
      <id>6</id>
      <posX>270</posX>
      <posY>165</posY>
      <width>200</width>
      <height>20</height>
      <label>867</label>
      <onleft>3</onleft>
      <onup>5</onup>
      <ondown>4</ondown>
      <textureFocus>check-box.png</textureFocus>
      <textureNoFocus>check-boxNF.png</textureNoFocus>
    </control>
  </controls>
</window>