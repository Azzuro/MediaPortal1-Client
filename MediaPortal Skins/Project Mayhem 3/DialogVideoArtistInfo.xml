<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>614</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>background image</description>
      <type>image</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
    </control>
    <control>
      <type>image</type>
      <id>1</id>
      <posX>60</posX>
      <posY>20</posY>
      <texture>-</texture>
    </control>
    <control>
      <description>Actor Name Label</description>
      <type>label</type>
      <id>1</id>
      <posX>100</posX>
      <posY>100</posY>
      <label>493</label>
      <textcolor>FFB2D4F5</textcolor>
    </control>
    <control>
      <description>Actor Name value</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>220</posX>
      <posY>100</posY>
      <width>400</width>
      <label>#Actor.Name</label>
      <font>font16</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>Born</description>
      <type>label</type>
      <id>1</id>
      <posX>100</posX>
      <posY>120</posY>
      <label>686</label>
      <textcolor>FFB2D4F5</textcolor>
    </control>
    <control>
      <description>Date of Birth</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>220</posX>
      <posY>120</posY>
      <width>400</width>
      <label>#Actor.DateOfBirth #Actor.PlaceOfBirth</label>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>Biography</description>
      <type>togglebutton</type>
      <id>3</id>
      <posX>100</posX>
      <posY>280</posY>
      <width>90</width>
      <height>32</height>
      <label>1011</label>
      <AltTextureFocus>togglebutton-off-focus.png</AltTextureFocus>
      <AltTextureNoFocus>togglebutton-off-nofocus.png</AltTextureNoFocus>
      <textureFocus>togglebutton-on-focus.png</textureFocus>
      <textureNoFocus>togglebutton-on-nofocus.png</textureNoFocus>
      <onleft>22</onleft>
      <onright>22</onright>
      <onup>2</onup>
      <ondown>4</ondown>
    </control>
    <control>
      <description>Movies</description>
      <type>togglebutton</type>
      <id>4</id>
      <posX>100</posX>
      <posY>316</posY>
      <width>90</width>
      <height>32</height>
      <label>342</label>
      <AltTextureFocus>togglebutton-off-focus.png</AltTextureFocus>
      <AltTextureNoFocus>togglebutton-off-nofocus.png</AltTextureNoFocus>
      <textureFocus>togglebutton-on-focus.png</textureFocus>
      <textureNoFocus>togglebutton-on-nofocus.png</textureNoFocus>
      <onleft>22</onleft>
      <onright>22</onright>
      <onup>3</onup>
      <ondown>5</ondown>
    </control>
    <control>
      <description>Biography value</description>
      <type>textboxscrollup</type>
      <id>20</id>
      <posX>220</posX>
      <posY>150</posY>
      <width>410</width>
      <height>400</height>
      <label>#Actor.Biography</label>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>Image logo</description>
      <type>image</type>
      <id>21</id>
      <posX>100</posX>
      <posY>150</posY>
      <width>100</width>
      <height>100</height>
      <texture>#thumb</texture>
      <colorkey>FFFF00FF</colorkey>
      <colordiffuse>FFFFFFFF</colordiffuse>
      <keepaspectratio>yes</keepaspectratio>
    </control>
    <control>
      <description>textarea</description>
      <type>textbox</type>
      <id>22</id>
      <posY>150</posY>
      <height>400</height>
      <onleft>4</onleft>
      <onright>4</onright>
      <onup>4</onup>
      <ondown>4</ondown>
      <label>#Actor.Movies</label>
    </control>
  </controls>
</window>