<window>
	<id>3009</id>
	<defaultcontrol>35</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control>
			<description>background image</description>
			<type>image</type>
			<id>0</id>
			<texture>context_background.png</texture>
			<width>-</width>
			<height>-</height>
			<posX>130</posX>
			<posY>130</posY>
		</control>
		<control>
			<description>exit button</description>
			<type>button</type>
			<id>34</id>
			<posX>560</posX>
			<posY>139</posY>
			<width>24</width>
			<height>16</height>
			<textureFocus>home-focus.gif</textureFocus>
			<textureNoFocus>button_nf.png</textureNoFocus>
			<label>-</label>
			<onleft>36</onleft>
			<onright>34</onright>
			<ondown>35</ondown>
			<onup>34</onup>
		</control>
		<control>
			<description>list to show channels</description>
			<type>listcontrol</type>
			<id>35</id>
			<posX>146</posX>
			<posY>177</posY>
			<width>444</width>
			<height>280</height>
			<textXOff>5</textXOff>
			<textYOff>20</textYOff>
			<textXOff2>430</textXOff2>
			<textYOff2>20</textYOff2>
			<textXOff3>50</textXOff3>
			<textYOff3>3</textYOff3>
			<IconXOff>12</IconXOff>
			<IconYOff>4</IconYOff>
			<spinPosX>530</spinPosX>
			<spinPosY>414</spinPosY>
			<spinWidth>16</spinWidth>
			<spinHeight>16</spinHeight>
			<itemWidth>35</itemWidth>
			<itemHeight>35</itemHeight>
			<textureFocus>recording_background_focus.png</textureFocus>
			<textureNoFocus>recording_background_nofocus.png</textureNoFocus>
			<spaceBetweenItems>3</spaceBetweenItems>
			<textureHeight>44</textureHeight>
			<keepaspectratio>no</keepaspectratio>
			<font>font12</font>
			<font2>font12</font2>
			<font3>font13B</font3>
			<remoteColor>ffffffff</remoteColor>   <!-- used for selected channel -->
			<onleft>36</onleft>
			<onright>34</onright>
			<onup>36</onup>
		</control>
		<control>
			<description>groups spin control</description>
			<type>spincontrol</type>
			<id>36</id>
			<posX>150</posX>
			<posY>137</posY>
			<showrange>no</showrange>
			<orientation>horizontal</orientation>
			<align>left</align>
			<reverse>yes</reverse>
			<font>font13</font>
			<textureUp>arrow_round_right_nofocus.png</textureUp>
			<textureDown>arrow_round_left_nofocus.png</textureDown>
			<textureUpFocus>arrow_round_right_focus.gif</textureUpFocus>
			<textureDownFocus>arrow_round_left_focus.gif</textureDownFocus>
			<ondown>35</ondown>
			<onup>36</onup>
			<onright>34</onright>
			<onleft>36</onleft>
		</control>
		<control>
			<description>time label</description>
			<id>0</id>
			<type>label</type>
			<posX>530</posX>
			<posY>135</posY>
			<align>right</align>
			<font>font13</font>
			<textcolor>white</textcolor>
			<label>#time</label>
		</control>
	</controls>
</window >