<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>4760</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.label:Flash Player</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <type>image</type>
      <description>BackGround</description>
      <id>1</id>
      <width>720</width>
      <height>576</height>
      <texture>background3.png</texture>
    </control>
    <control>
      <type>group</type>
      <layout>StackLayout</layout>
      <description>group element</description>
      <animation>FlyInFromLeft</animation>
      <posX>5</posX>
      <posY>125</posY>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>2</id>
        <label>Back</label>
        <onup>17</onup>
        <ondown>3</ondown>
        <onright>99</onright>
      </control>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>3</id>
        <label>Play</label>
        <onup>2</onup>
        <ondown>4</ondown>
        <onright>99</onright>
      </control>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>4</id>
        <label>Pause</label>
        <onup>3</onup>
        <ondown>5</ondown>
        <onright>99</onright>
      </control>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>5</id>
        <label>Stop</label>
        <onup>4</onup>
        <ondown>6</ondown>
        <onright>99</onright>
      </control>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>6</id>
        <label>Jmp Forward</label>
        <onup>5</onup>
        <ondown>7</ondown>
        <onright>99</onright>
      </control>
      <control>
        <description>Back Button</description>
        <type>button</type>
        <id>7</id>
        <label>Jump Backward</label>
        <onup>6</onup>
        <ondown>2</ondown>
        <onright>99</onright>
      </control>
    </control>
    <control>
      <description>TV background</description>
      <type>image</type>
      <id>1</id>
      <posX>281</posX>
      <posY>125</posY>
      <width>396</width>
      <height>335</height>
      <colordiffuse>60ffffff</colordiffuse>
      <texture>tv_background.png</texture>
    </control>
    <control>
      <description>video window</description>
      <type>videowindow</type>
      <id>99</id>
      <posX>300</posX>
      <posY>145</posY>
      <width>350</width>
      <height>260</height>
      <onleft>2</onleft>
      <onright>2</onright>
      <action>18</action>
      <textureFocus>tv_green_border.png</textureFocus>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>290</posX>
      <posY>453</posY>
      <width>250</width>
      <label>#TV.View.title</label>
      <textcolor>White</textcolor>
    </control>
    <control>
      <description>Channel label</description>
      <type>label</type>
      <id>13</id>
      <posX>660</posX>
      <posY>453</posY>
      <label />
      <align>right</align>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>13</id>
      <posX>290</posX>
      <posY>473</posY>
      <label>#TV.View.start-#TV.View.stop</label>
      <align>left</align>
      <textcolor>FFB2D4F5</textcolor>
    </control>
    <control>
      <description>Current Channelname</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>660</posX>
      <posY>473</posY>
      <width>90</width>
      <label>#TV.View.channel</label>
      <align>right</align>
      <textcolor>FFB2D4F5</textcolor>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>290</posX>
      <posY>493</posY>
      <width>350</width>
      <height>90</height>
      <seperator>----------------------------------------------------------------------------------------------</seperator>
      <label>#TV.View.description</label>
      <textcolor>White</textcolor>
    </control>
    <control>
      <description>Progress background</description>
      <type>image</type>
      <id>1</id>
      <posX>300</posX>
      <posY>415</posY>
      <width>350</width>
      <texture>osd_progress_background.png</texture>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>20</id>
      <posX>289</posX>
      <posY>414</posY>
      <width>372</width>
      <height>25</height>
      <label>#TV.View.Percentage</label>
      <visible>yes</visible>
    </control>
  </controls>
</window>