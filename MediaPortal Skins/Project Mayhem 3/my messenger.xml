<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>32</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#background.image:background3.png</define>
  <define>#header.image:Back_messenger.png</define>
  <define>#header.label:902</define>
  <controls>
    <import>common.window.xml</import>
    <control>
      <description>Selected item Label</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>690</posX>
      <width>360</width>
      <label>#selecteditem</label>
      <align>right</align>
    </control>
    <control>
      <description>button group</description>
      <type>group</type>
      <posX>5</posX>
      <posY>125</posY>
      <layout>StackLayout</layout>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <control>
        <description>View-As button</description>
        <type>button</type>
        <id>2</id>
        <label>100</label>
        <onleft>2</onleft>
        <onright>50</onright>
        <onup>20</onup>
        <ondown>3</ondown>
      </control>
      <control>
        <type>sortbutton</type>
        <id>3</id>
        <label>125</label>
        <onleft>3</onleft>
        <onright>50</onright>
        <onup>2</onup>
        <ondown>5</ondown>
      </control>
      <control>
        <description>Status button</description>
        <type>selectbutton</type>
        <id>5</id>
        <label>936</label>
        <onleft>3</onleft>
        <ondown>6</ondown>
        <subitems>
          <subitem>948</subitem>
          <subitem>949</subitem>
          <subitem>950</subitem>
          <subitem>951</subitem>
          <subitem>952</subitem>
          <subitem>953</subitem>
          <subitem>954</subitem>
        </subitems>
      </control>
      <control>
        <description>Connect button</description>
        <type>button</type>
        <id>6</id>
        <label>903</label>
        <onleft>6</onleft>
        <onright>50</onright>
        <onup>5</onup>
        <ondown>2</ondown>
      </control>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation>FlyInFromTop</animation>
      <control>
        <description>listcontrol</description>
        <type>listcontrol</type>
        <id>50</id>
        <onleft>51</onleft>
        <onright>51</onright>
        <onup>2</onup>
        <ondown>2</ondown>
      </control>
      <control>
        <description>Thumbnail Panel</description>
        <type>thumbnailpanel</type>
        <id>51</id>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
      </control>
    </control>
  </controls>
</window>