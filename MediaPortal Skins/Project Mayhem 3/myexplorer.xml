<window>
	<id>770</id>
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<define>#header.label:#explorer_title</define>
	<controls>
		<import>common.window.xml</import>
	 <control>
         <type>image</type>

         <description>BG</description>

         <id>1</id>

         <posX>0</posX>

         <posY>0</posY>

         <width>720</width>

         <height>576</height>

         <texture>background3.png</texture>
      </control>
       <control>
         <type>image</type>

         <description>BG</description>

         <id>1</id>

         <posX>0</posX>

         <posY>0</posY>

         <width>720</width>

         <height>107</height>

         <texture>Back_setting.png</texture>
      </control>
		<control>
			<description>My explorer File Size label</description>
			<type>label</type>
			<id>1</id>
			<posX>40</posX>
			<posY>530</posY>
			<label>#explorer_size</label>
			<font>font16</font>
			<align>left</align>
			<textcolor>White</textcolor>
		</control>
		<control>
			<type>group</type>
			<description>group element</description>
			<animation>FlyInFromLeft</animation>
			<posX>5</posX>
			<posY>125</posY>
			<layout>StackLayout</layout>
			<control>
				<description>select source</description>
				<type>button</type>
				<id>2</id>
				<label>2201</label>
				<onleft>2</onleft>
				<onright>20</onright>
				<onup>17</onup>
				<ondown>3</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
			<control>
				<description>select destination</description>
				<type>button</type>
				<id>3</id>
				<label>2202</label>
				<onleft>3</onleft>
				<onright>20</onright>
				<onup>2</onup>
				<ondown>4</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
			<control>
				<description>copy files</description>
				<type>button</type>
				<id>4</id>
				<label>2203</label>
				<onleft>2</onleft>
				<onright>20</onright>
				<onup>3</onup>
				<ondown>5</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
			<control>
				<description>make dir</description>
				<type>button</type>
				<id>5</id>
				<label>2204</label>
				<onleft>5</onleft>
				<onright>20</onright>
				<onup>4</onup>
				<ondown>6</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
			<control>
				<description>reset selection</description>
				<type>button</type>
				<id>6</id>
				<label>2205</label>
				<onleft>5</onleft>
				<onright>20</onright>
				<onup>5</onup>
				<ondown>7</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
			<control>
				<description>mark all</description>
				<type>button</type>
				<id>7</id>
				<label>2206</label>
				<onleft>6</onleft>
				<onright>20</onright>
				<onup>6</onup>
				<ondown>2</ondown>
				<textXOff>17</textXOff>
  				<alignY>center</alignY>
			</control>
		</control>
		<control>
			<description>file listcontrol</description>
			<type>listcontrol</type>
			<id>20</id>
			<posX>270</posX>
			<posY>97</posY>
			<height>450</height>
			<onright>2</onright>
			<onleft>2</onleft>
			<onup>4</onup>
			<ondown>2</ondown>
		</control>
	</controls>
</window>