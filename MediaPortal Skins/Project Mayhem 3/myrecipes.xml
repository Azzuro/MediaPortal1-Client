<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>750</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#background.image:background3.png</define>
  <define>#header.image:Back_recipes.png</define>
  <define>#header.label:10</define>
  <controls>
    <import>common.window.xml</import>
    <control>
      <description>yield interval spin control</description>
      <type>spincontrol</type>
      <id>9</id>
      <posX>250</posX>
      <posY>120</posY>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <textureUp>arrow_round_up_nofocus.png</textureUp>
      <textureDown>arrow_round_down_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
      <onleft>2</onleft>
      <onright>9</onright>
    </control>
    <control>
      <description>button group</description>
      <type>group</type>
      <posX>5</posX>
      <posY>125</posY>
      <layout>StackLayout</layout>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <control>
        <description>back button</description>
        <type>button</type>
        <id>2</id>
        <label>712</label>
        <onleft>2</onleft>
        <onright>9</onright>
        <onup>17</onup>
        <ondown>3</ondown>
      </control>
      <control>
        <description>search button type</description>
        <type>button</type>
        <id>3</id>
        <label>2051</label>
        <onleft>3</onleft>
        <onright>10</onright>
        <onup>2</onup>
        <ondown>5</ondown>
      </control>
      <control>
        <description>search button</description>
        <type>button</type>
        <id>5</id>
        <label>137</label>
        <onleft>5</onleft>
        <onright>10</onright>
        <onup>3</onup>
        <ondown>6</ondown>
      </control>
      <control>
        <description>favor button</description>
        <type>button</type>
        <id>6</id>
        <label>2013</label>
        <onleft>6</onleft>
        <onright>10</onright>
        <onup>5</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>delete button</description>
        <type>button</type>
        <id>7</id>
        <label>2005</label>
        <onleft>7</onleft>
        <onright>10</onright>
