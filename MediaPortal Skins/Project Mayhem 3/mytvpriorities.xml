<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>607</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.label:877</define>
  <controls>
    <import>common.window.xml</import>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
      <texture>background3.png</texture>
    </control>
    <import>common.time.xml</import>
    <control>
      <description>Number of Files Label</description>
      <type>label</type>
      <id>12</id>
      <posX>545</posX>
      <posY>526</posY>
      <label>#itemcount</label>
      <align>right</align>
      <font>font10</font>
      <textcolor>FFFFFFFF</textcolor>
      <visible>no</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <type>group</type>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <description>group element</description>
      <control>
        <description>scheduler recordings listcontrol</description>
        <type>updownlistcontrol</type>
        <id>10</id>
        <posX>225</posX>
        <posY>120</posY>
        <width>445</width>
        <height>322</height>
        <textXOff>15</textXOff>
        <textYOff>2</textYOff>
        <textXOff2>420</textXOff2>
        <textYOff2>51</textYOff2>
        <textXOff3>65</textXOff3>
        <textYOff3>52</textYOff3>
        <IconXOff>6</IconXOff>
        <IconYOff>4</IconYOff>
        <itemWidth>48</itemWidth>
        <itemHeight>48</itemHeight>
        <textureFocus>blue-rectangle.png</textureFocus>
        <textureNofocus>blue-rectangleNF.png</textureNofocus>
        <spaceBetweenItems>10</spaceBetweenItems>
        <textureHeight>70</textureHeight>
        <keepaspectratio>no</keepaspectratio>
        <font2>font10</font2>
        <font3>font10</font3>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
      </control>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>225</posX>
      <posY>360</posY>
      <width>250</width>
      <label>#TV.Scheduled.Title</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>245</posX>
      <posY>380</posY>
      <label>#TV.Scheduled.Time</label>
      <align>left</align>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>245</posX>
      <posY>400</posY>
      <width>430</width>
      <height>85</height>
      <label>#TV.Scheduled.Description</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>genre</description>
      <type>label</type>
      <id>17</id>
      <posX>660</posX>
      <posY>380</posY>
      <label>#TV.Scheduled.Genre</label>
      <align>right</align>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>TV background</description>
      <type>image</type>
      <id>1</id>
      <posX>10</posX>
      <posY>468</posY>
      <width>180</width>
      <height>108</height>
      <colordiffuse>60ffffff</colordiffuse>
      <texture>-</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>TV preview window</description>
      <type>videowindow</type>
      <id>99</id>
      <posX>30</posX>
      <posY>422</posY>
      <width>150</width>
      <height>100</height>
      <onup>7</onup>
      <action>18</action>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <textureFocus>tv_green_border.png</textureFocus>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
  </controls>
</window>