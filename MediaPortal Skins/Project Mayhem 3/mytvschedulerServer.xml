<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>601</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#background.image:background3.png</define>
  <define>#header.image:Back_tv.png</define>
  <define>#header.label:646</define>
  <controls>
    <import>common.window.xml</import>
    <control>
      <description>button group</description>
      <type>group</type>
      <posX>5</posX>
      <posY>125</posY>
      <layout>StackLayout</layout>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <control>
        <type>sortbutton</type>
        <id>2</id>
        <label>620</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>17</onup>
        <ondown>11</ondown>
      </control>
      <control>
        <description>Toggle button series</description>
        <type>togglebutton</type>
        <id>11</id>
        <label>3019</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>2</onup>
        <ondown>6</ondown>
      </control>
      <control>
        <description>New button</description>
        <type>button</type>
        <id>6</id>
        <label>617</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>11</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>Cleanup</description>
        <type>button</type>
        <id>7</id>
        <label>624</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>6</onup>
        <ondown>8</ondown>
      </control>
      <control>
        <description>Priorities</description>
        <type>button</type>
        <id>8</id>
        <label>877</label>
        <hyperlink>607</hyperlink>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>7</onup>
        <ondown>9</ondown>
      </control>
      <control>
        <description>Conflicts</description>
        <type>button</type>
        <id>9</id>
        <label>892</label>
        <hyperlink>608</hyperlink>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>8</onup>
        <ondown>99</ondown>
      </control>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <control>
        <description>scheduler recordings listcontrol</description>
        <type>listcontrol</type>
        <id>10</id>
        <height>370</height>
        <textXOff>50</textXOff>
        <textXOff2>420</textXOff2>
        <textYOff2>29</textYOff2>
        <textXOff3>100</textXOff3>
        <textYOff3>29</textYOff3>
        <IconXOff>12</IconXOff>
        <PinIconXOff>45</PinIconXOff>
        <PinIconYOff>19</PinIconYOff>
        <spinPosX>606</spinPosX>
        <spinPosY>418</spinPosY>
        <itemWidth>35</itemWidth>
        <itemHeight>35</itemHeight>
        <textureFocus>recording_background_focus.png</textureFocus>
        <textureNoFocus>recording_background_nofocus.png</textureNoFocus>
        <spaceBetweenItems>5</spaceBetweenItems>
        <textureHeight>50</textureHeight>
        <keepaspectratio>no</keepaspectratio>
        <font2>font10</font2>
        <font3>font10</font3>
        <onleft>2</onleft>
        <onright>2</onright>
        <!--         <onup>2</onup>
        <ondown>2</ondown> -->
      </control>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>500</posX>
      <posY>440</posY>
      <width>250</width>
      <label>#TV.Scheduled.Title</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>500</posX>
      <posY>460</posY>
      <label>#TV.Scheduled.Time</label>
      <align>left</align>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>260</posX>
      <posY>485</posY>
      <width>430</width>
      <height>65</height>
      <label>#TV.Scheduled.Description</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
    <control>
      <description>channel</description>
      <type>label</type>
      <id>17</id>
      <posX>677</posX>
      <posY>460</posY>
      <label>#TV.Scheduled.Channel</label>
      <align>right</align>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
    </control>
  </controls>
</window>