        <description>Graphical Thumbpanel</description>
        <type>thumbnailpanel</type>
        <id>50</id>
        <posX>20</posX>
        <posY>20</posY>
        <width>680</width>
        <height>536</height>
        <!--<spinWidth>24</spinWidth>
          <spinHeight>24</spinHeight>
          <spinPosX>1200</spinPosX>
          <spinPosY>623</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ccffffff</spinColor>-->
        <!--<imageFolder>tvseries_banner_nofocus.png</imageFolder>
          <imageFolderFocus>tvseries_banner_nofocus.png</imageFolderFocus>-->
        <!-- Text -->
        <selectedColor>00ffffff</selectedColor>
        <textcolor>ffffffff</textcolor>
        <colordiffuse>ffffffff</colordiffuse>
        <downloadColor>#default.color</downloadColor>
        <remoteColor>#watched.color</remoteColor>
        <textcolor>#notpresent.color</textcolor>
        <textcolor2>#notpresent.color</textcolor2>
        <textcolor3>#notpresent.color</textcolor3>
