<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>9812</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <!-- ~~~~~~~~~~~~~~~~~~~~ ALL BACKGROUNDS ~~~~~~~~~~~~~~~~~~~~ -->
    <control>
      <description>Fan Art Image</description>
      <type>image</type>
      <id>4560</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>720</width>
      <!--<height>576</height>-->
      <!--<width>1280</width>
      <height>720</height>-->
