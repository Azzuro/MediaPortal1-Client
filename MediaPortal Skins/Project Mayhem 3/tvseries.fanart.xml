        <itemWidthBig>333</itemWidthBig>
        <itemHeightBig>203</itemHeightBig>
        <!-- boxes -->
        <textureWidthBig>333</textureWidthBig>
        <textureHeightBig>223</textureHeightBig>
        <!-- banners -->
        <thumbWidthBig>300</thumbWidthBig>
        <thumbHeightBig>168</thumbHeightBig>
        <thumbPosXBig>16</thumbPosXBig>
        <thumbPosYBig>26</thumbPosYBig>
