<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <type>group</type>

      <animation effect="slide" time="350" end="300,0">WindowClose</animation>
      <animation effect="slide" time="450" start="300,0">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="fade" time="500">WindowOpen</animation>
      <description>group element</description>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <posX>375</posX>
          <width>950</width>
          <textureHeight>45</textureHeight>
          <font>font16</font>
          <textXOff>82</textXOff>
          <textXOff2>872</textXOff2>
          <itemWidth>40</itemWidth>
          <itemHeight>37</itemHeight>
          <IconXOff>64</IconXOff>
          <IconYOff>4</IconYOff>
          <scrollOffset>2</scrollOffset>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>90</posX>
          <width>1250</width>
          <font>font12</font>
          <thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
          <enableFocusZoom>yes</enableFocusZoom>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <posX>80</posX>
          <posY>260</posY>
          <width>1250</width>
          <onup>2</onup>
          <thumbWidth>179</thumbWidth>
          <thumbHeight>134</thumbHeight>
          <thumbPosX>16</thumbPosX>
          <thumbPosY>12</thumbPosY>
          <imageFolderFocus>Framefocus.png</imageFolderFocus>
          <frame>Picture_cover.png</frame>
          <frameFocus>Pictureframe_cover.png</frameFocus>
          <itemWidth>246</itemWidth>
          <itemHeight>210</itemHeight>
          <textureWidth>237</textureWidth>
          <textureHeight>289</textureHeight>
          <font>font11</font>
          <textXOff>8</textXOff>
		  <textYOff>-25</textYOff>
          <selectedColor>00ffffff</selectedColor>
          <textcolor>00ffffff</textcolor>
          <colordiffuse>00ffffff</colordiffuse>
          <remoteColor>00ffffff</remoteColor>
          <downloadColor>00ffffff</downloadColor>
          <background>Coverbig.png</background>
          <thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="140,140" time="300">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="140,140" end="100,100" time="300">unfocus</thumbAnimation>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <scrollOffset>1</scrollOffset>
        </control>
      </control>
    </control>
  </controls>
</window>