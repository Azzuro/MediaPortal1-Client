<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>34</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <disabletopbar>no</disabletopbar>
  <controls>
    <control>
      <description>BG</description>
      <type>multiimage</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
      <imagepath>back</imagepath>
      <timeperimage>15000</timeperimage>
      <fadetime>10000</fadetime>
      <loop>yes</loop>
      <randomize>false</randomize>
      <animation effect="fade" time="200">VisibleChange</animation>
    </control>
    <control>
      <description>header logo</description>
      <type>image</type>
      <id>1</id>
      <posX>60</posX>
      <posY>20</posY>
      <texture>plugins_logo.png</texture>
    </control>
    <control>
      <description>MPlogo</description>
      <type>image</type>
      <id>1</id>
      <posX>520</posX>
      <posY>500</posY>
      <texture>hp_start.png</texture>
    </control>
    <control>
      <description>Menu</description>
      <type>menu</type>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="500">WindowClose</animation>
      <animation effect="zoom" start="20,20" end="100,100" center="384,288" time="500">WindowOpen</animation>
      <animation effect="zoom" start="100,100" end="20,20" center="384,288" time="500">WindowClose</animation>
      <id>50</id>
      <posX>370</posX>
      <posY>120</posY>
      <width>310</width>
      <textcolor>ffffffff</textcolor>
      <textColorNoFocus>fffffff0</textColorNoFocus>
      <dimColor>80B36162</dimColor>
      <buttonTextXOff>40</buttonTextXOff>
      <buttonTextYOff>38</buttonTextYOff>
      <buttonOffset>25</buttonOffset>
      <buttonFont>font18</buttonFont>
      <buttonHeight>84</buttonHeight>
      <spaceBetweenButtons>18</spaceBetweenButtons>
      <numberOfButtons>3</numberOfButtons>
      <textureButtonFocus>button_empty.png</textureButtonFocus>
      <textureButtonNoFocus>buttonNF_empty.png</textureButtonNoFocus>
      <hoverX>60</hoverX>
      <hoverY>225</hoverY>
      <hoverWidth>432</hoverWidth>
      <hoverHeight>336</hoverHeight>
      <hoverKeepAspectratio>yes</hoverKeepAspectratio>
      <scrollTime>200</scrollTime>
      <scrollTimeMin>300</scrollTimeMin>
      <textureBackground>-</textureBackground>
      <onright>13</onright>
      <onleft>10</onleft>
    </control>
    <!-- Date and Time -->
    <!-- TOPBAR Buttons -->
  </controls>
</window>