<window>
	<id>5334</id>
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>yes</allowoverlay>	
	<define>#header.label:My Planner</define>
	<define>#header.text:planner</define>
	<define>#header.hover:hover_my planner.png</define>
    <controls>
		<import>common.window.default.xml</import>
     	<control>
		<type>group</type>
		<description>group element</description>
		<animation>FlyInFromLeft</animation>
		<layout>StackLayout(2)</layout>
		<posX>60</posX>
		<posY>97</posY>
		<control>
			<description>New Date</description>
			<type>button</type>
			<id>2</id>
			<label>New Date</label>
			<onleft>4</onleft>			
			<onright>3</onright>
			<onup>6</onup>			
			<ondown>5</ondown>
		</control>
		<control>
			<description>Edit Date</description>
			<type>button</type>
			<id>3</id>
			<label>Edit Date</label>
			<onleft>2</onleft>
			<onright>4</onright>
			<onup>6</onup>			
			<ondown>5</ondown>
		</control>
		<control>
			<description>Delete Date</description>
			<type>button</type>
			<id>4</id>
			<label>Delete Date</label>
			<onleft>3</onleft>
			<onright>2</onright>			
			<onup>6</onup>				
			<ondown>5</ondown>
		</control>
		<control>
			<description>Back</description>
			<type>button</type>
			<id>6</id>
			<posX>50</posX>
			<posY>525</posY>
			<label>Calendar</label>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>5</onup>
			<ondown>2</ondown>
		</control>
	</control>
		<control>
			<description>DateList</description>
			<type>listcontrol</type>
			<id>5</id>
			<onleft>6</onleft>
			<onright>6</onright>	
			<onup>2</onup>
			<ondown>6</ondown>
		</control>	
	</controls>
</window>