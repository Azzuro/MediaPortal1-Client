      <type>label</type>
      <id>200</id>
      <posX>690</posX>
      <posY>30</posY>
      <label>#date</label>
      <align>right</align>
      <font>font13</font>
      <textcolor>ffa9d0f7</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Time</description>
      <type>label</type>
      <id>201</id>
      <posX>690</posX>
      <posY>50</posY>
