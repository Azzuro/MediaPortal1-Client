<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <!--<posX>400</posX>
          <posY>121</posY>-->
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
          <width>425</width>
          <height>370</height>
          <!--<PinIconWidth>35</PinIconWidth>
					<PinIconHeight>25</PinIconHeight>
					<PinIconXOff>49</PinIconXOff>
					<PinIconYOff>11</PinIconYOff>
					<itemWidth>32</itemWidth>
        			<itemHeight>32</itemHeight>
        			<IconXOff>12</IconXOff>
        			<IconYOff>4</IconYOff>
        			<textXOff>48</textXOff>
        			<textYOff>3</textYOff>
        			<textXOff2>0</textXOff2>
        			<textYOff2>5</textYOff2>
        			<font2>font11</font2>-->
          <keepaspectratio>yes</keepaspectratio>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <width>425</width>
          <height>370</height>
          <font>font13</font>
          <font2>font13</font2>
          <selectedColor>80000000</selectedColor>
          <textcolor>ffffffff</textcolor>
          <selectedColor2>ffffffff</selectedColor2>
          <textcolor2>ffffffff</textcolor2>
          <textXOff2>420</textXOff2>
          <textXOff3>111</textXOff3>
          <textYOff2>60</textYOff2>
          <itemWidth>72</itemWidth>
          <itemHeight>72</itemHeight>
          <!--<IconXOff>12</IconXOff>
          <IconYOff>9</IconYOff>-->
          <spaceBetweenItems>14</spaceBetweenItems>
          <textureHeight>90</textureHeight>
          <scrollOffset>1</scrollOffset>
          <textureFocus>album_background_focus.png</textureFocus>
          <textureNoFocus>album_background_nofocus.png</textureNoFocus>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>        
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>260</posX>
          <posY>105</posY>
          <width>425</width>
          <height>370</height>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>50</onright>
          <itemWidth>140</itemWidth>
          <itemHeight>77</itemHeight>
          <textureWidth>100</textureWidth>
          <textureHeight>60</textureHeight>
          <thumbWidth>89</thumbWidth>
          <thumbHeight>50</thumbHeight>
          <thumbPosX>5</thumbPosX>
          <thumbPosY>3</thumbPosY>
          <itemWidthBig>210</itemWidthBig>
          <itemHeightBig>166</itemHeightBig>
          <textureWidthBig>177</textureWidthBig>
          <textureHeightBig>102</textureHeightBig>
          <thumbWidthBig>160</thumbWidthBig>
          <thumbHeightBig>92</thumbHeightBig>
          <thumbPosXBig>8</thumbPosXBig>
          <thumbPosYBig>5</thumbPosYBig>
          <zoomXPixels>0</zoomXPixels>
          <zoomYPixels>0</zoomYPixels>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>false</enableFocusZoom>
          <keepaspectratio>no</keepaspectratio>
          <hideUnfocusTexture>no</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>yes</renderUnfocusText>
        </control>
      </control>
