<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <controls>
    <!-- these elements need condition visibility for OSD IS VISIBLE -->
    <control>
      <description>background botton</description>
      <id>300</id>
      <type>image</type>
      <posX>84</posX>
      <posY>465</posY>
      <width>565</width>
      <height>111</height>
      <texture>OSD-Smalbackground.png</texture>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
      <visible>[player.rewinding|Control.IsVisible(10)|player.forwarding|window.ispauseosdvisible]+!window.isosdvisible</visible>
    </control>
    <control>
      <description>TV Logo</description>
      <type>image</type>
      <id>0</id>
      <posX>590</posX>
      <posY>508</posY>
      <width>39</width>
      <height>56</height>
      <texture>#Play.Current.Thumb</texture>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
      <visible>[player.rewinding|Control.IsVisible(10)|player.forwarding|window.ispauseosdvisible]+!window.isosdvisible</visible>
      <animation effect="fade" time="150">windowopen</animation>
    </control>
    <control>
      <description>Movie Name</description>
      <id>36</id>
      <type>fadelabel</type>
      <width>289</width>
      <height>29</height>
      <posX>101</posX>
      <posY>476</posY>
      <font>font14</font>
      <label>#Play.Current.Title</label>
      <textcolor>white</textcolor>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
      <visible>[player.rewinding|Control.IsVisible(10)|player.forwarding|window.ispauseosdvisible]+!window.isosdvisible</visible>
    </control>
    <control>
      <description>Stripe</description>
      <type>image</type>
      <posX>95</posX>
      <posY>504</posY>
      <width>538</width>
      <height>2</height>
      <texture>OSD_Stripe.png</texture>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
      <visible>[player.rewinding|Control.IsVisible(10)|player.forwarding|window.ispauseosdvisible]+!window.isosdvisible</visible>
    </control>
    <control>
      <description>Current Time Info</description>
      <type>label</type>
      <posX>632</posX>
      <posY>477</posY>
      <label>#time</label>
      <font>font14</font>
      <align>right</align>
      <textcolor>white</textcolor>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
      <visible>[player.rewinding|Control.IsVisible(10)|player.forwarding|window.ispauseosdvisible]+!window.isosdvisible</visible>
    </control>
    <control>
      <description>progress bar background</description>
      <type>image</type>
      <posX>95</posX>
      <posY>516</posY>
      <width>467</width>
      <height>20</height>
      <texture>osd_progress_background.png</texture>
      <visible>window.isosdvisible|window.ispauseosdvisible|player.rewinding|player.forwarding|Control.IsVisible(10)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>1</id>
      <posX>92</posX>
      <posY>517</posY>
      <width>462</width>
      <height>20</height>
      <label>#play.progressbar.percentage</label>
      <visible>[window.isosdvisible|window.ispauseosdvisible|player.rewinding|player.forwarding|Control.IsVisible(10)]+!window.isvisible(602)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>Start Time Info</description>
      <type>label</type>
      <posX>115</posX>
      <posY>552</posY>
      <label>#play.progressbar.currentplaytime</label>
      <align>center</align>
      <font>font12</font>
      <textcolor>white</textcolor>
      <visible>window.isosdvisible|window.ispauseosdvisible|player.rewinding|player.forwarding|Control.IsVisible(10)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>End Time Info</description>
      <type>label</type>
      <posX>542</posX>
      <posY>552</posY>
      <label>#play.progressbar.currentremaining</label>
      <align>center</align>
      <font>font12</font>
      <textcolor>white</textcolor>
      <visible>window.isosdvisible|window.ispauseosdvisible|player.rewinding|player.forwarding|Control.IsVisible(10)</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
