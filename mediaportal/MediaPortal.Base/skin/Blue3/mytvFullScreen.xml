<window>
  <id>602</id>
  <defaultcontrol>13</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#play.progressbar.percentage:#TV.View.Percentage</define>
  <define>#play.progressbar.currentplaytime:#TV.View.start</define>
  <define>#play.progressbar.currentremaining:#TV.View.stop</define>
  <controls>
    <import>common.play.progressbar.xml</import>
    <import>common.play.fullscreen.xml</import>
    <control>
      <description>channelname</description>
      <type>label</type>
      <id>14</id>
      <posX>37</posX>
      <posY>29</posY>
      <width>189</width>
      <label>-</label>
      <align>left</align>
      <font>font13</font>
      <textcolor>White</textcolor>
      <visible>no</visible>
      <animation effect="fade" time="250">visiblechange</animation>
    </control>
    <control>
      <description>TV Logo</description>
      <type>image</type>
      <id>0</id>
      <posX>590</posX>
      <posY>508</posY>
      <width>39</width>
      <height>56</height>
      <texture>#TV.View.thumb</texture>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <visible>player.rewinding|Control.IsVisible(10)|player.forwarding|player.paused+!window.isosdvisible</visible>
      <animation effect="fade" time="150">windowopen</animation>
    </control>
