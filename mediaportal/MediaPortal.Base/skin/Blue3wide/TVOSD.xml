      <posY>650</posY>
      <width>35</width>
      <height>25</height>
      <textureFocus>arrow_round_up_focus.png</textureFocus>
      <textureNoFocus>arrow_round_up_nofocus.png</textureNoFocus>
      <label>-</label>
      <onleft>32</onleft>
      <onright>31</onright>
      <onup>34</onup>
      <ondown>31</ondown>
      <visible>no</visible>
