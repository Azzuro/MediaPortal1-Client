      <label>-</label>
      <onleft>214</onleft>
      <onright>34</onright>
      <onup>33</onup>
      <ondown>32</ondown>
      <visible>no</visible>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
