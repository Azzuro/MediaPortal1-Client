<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>3003</id>
  <defaultcontrol>213</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#play.progressbar.percentage:#TV.View.Percentage</define>
  <define>#play.progressbar.currentplaytime:#TV.View.start</define>
  <define>#play.progressbar.currentremaining:#TV.View.stop</define>
  <define>#play.osd.background:osd_bg_bottom.png</define>
  <controls>
    <import>common.play.osd.xml</import>
    <control>
      <description>TV Logo</description>
      <type>image</type>
      <posX>71</posX>
      <posY>435</posY>
      <width>88</width>
      <height>62</height>
      <texture>#TV.View.thumb</texture>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>TV Progress Bar</description>
      <type>tvprogress</type>
      <id>101</id>
      <posX>174</posX>
      <posY>37</posY>
      <width>714</width>
      <height>25</height>
      <toptexture>osd_progress_indicator.png</toptexture>
      <TextureOffsetY>27</TextureOffsetY>
      <bottomtexture>-</bottomtexture>
      <texturetick>-</texturetick>
      <lefttexture>-</lefttexture>
      <midtexture>-</midtexture>
      <righttexture>-</righttexture>
      <logotexture>-</logotexture>
      <fillbackgroundtexture>-</fillbackgroundtexture>
      <fillbgxoff>17</fillbgxoff>
      <fillbgyoff>12</fillbgyoff>
      <filltexture1>osd_progress_mid_red.png</filltexture1>
      <filltexture2>osd_progress_mid.png</filltexture2>
      <filltexture3>osd_progress_mid_orange.png</filltexture3>
      <fillheight>25</fillheight>
      <label>#TV.Record.percent1</label>
      <label1>#TV.Record.percent2</label1>
      <label2>#TV.Record.percent3</label2>
      <font>font13</font>
      <textcolor>FF000000</textcolor>
    </control>
    <control>
      <description>rec logo</description>
      <type>image</type>
      <id>39</id>
      <posX>666</posX>
      <posY>437</posY>
      <texture>rec.png</texture>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>Channel Name</description>
      <type>textbox</type>
      <width>551</width>
      <height>37</height>
      <posX>195</posX>
      <posY>425</posY>
      <align>left</align>
      <font>font13</font>
      <label>#TV.View.channel</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>On tv NOW</description>
      <id>36</id>
      <type>textbox</type>
      <width>515</width>
      <height>37</height>
      <posX>195</posX>
      <posY>460</posY>
      <align>left</align>
      <font>font13</font>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">visiblechange</animation>
      <animation effect="fade" time="250">windowopen</animation>
    </control>
    <control>
      <description>Plot</description>
