<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      <animation effect="fade" time="150">WindowOpen</animation>
      <animation effect="fade" time="150">WindowClose</animation>
      <animation effect="slide" time="150" start="0,-300">WindowOpen</animation>
      <animation effect="slide" time="150" end="800,0">WindowClose</animation>
      <animation effect="rotatey" start="20" end="0" center="525,0" tween="back" ease="easeout" time="300" reversible="false">hidden</animation>
      <animation effect="rotatey" start="0" end="20" center="525,0" tween="back" ease="easeout" time="300" reversible="false">visible</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <width>755</width>
          <height>593</height>
          <font>font13</font>
          <font2>font13</font2>
          <selectedColor>80000000</selectedColor>
          <textcolor>ffffffff</textcolor>
          <selectedColor2>ffffffff</selectedColor2>
          <textcolor2>ffffffff</textcolor2>
          <textXOff2>746</textXOff2>
          <textYOff2>75</textYOff2>
          <itemWidth>90</itemWidth>
          <itemHeight>90</itemHeight>
          <IconXOff>12</IconXOff>
          <IconYOff>9</IconYOff>
          <spaceBetweenItems>24</spaceBetweenItems>
          <textureHeight>112</textureHeight>
          <scrollOffset>1</scrollOffset>
          <textureFocus>album_background_focus.png</textureFocus>
          <textureNoFocus>album_background_nofocus.png</textureNoFocus>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>88</posX>
          <posY>105</posY>
          <width>1173</width>
          <height>588</height>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <textureWidthBig>155</textureWidthBig>
          <textureHeightBig>150</textureHeightBig>
          <itemWidthBig>191</itemWidthBig>
          <itemHeightBig>185</itemHeightBig>
          <thumbWidthBig>144</thumbWidthBig>
          <thumbHeightBig>144</thumbHeightBig>
          <thumbPosXBig>5</thumbPosXBig>
          <thumbPosYBig>3</thumbPosYBig>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <zoomXPixels>35</zoomXPixels>
          <zoomYPixels>35</zoomYPixels>
          <hideUnfocusTexture>no</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>yes</renderUnfocusText>
          <showFrame>true</showFrame>
		  <frameNoFocus>Thumb_frame_nofocus.png</frameNoFocus>
		  <frameFocus>Thumb_frame_nofocus.png</frameFocus>
		  <font>font9</font>
		  <dimColor>ffffffff</dimColor>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <posX>0</posX>
          <posY>255</posY>
          <width>1300</width>
          <height>720</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <thumbWidth>200</thumbWidth>
          <thumbHeight>200</thumbHeight>
          <thumbPosX>4</thumbPosX>
          <thumbPosY>4</thumbPosY>
          <itemWidth>250</itemWidth>
          <itemHeight>231</itemHeight>
          <textureWidth>210</textureWidth>
          <textureHeight>210</textureHeight>
          <imageFolderFocus>Picture_cover.png</imageFolderFocus>
          <frame>Picture_cover.png</frame>
          <frameFocus>Picture_cover.png</frameFocus>
          <background>emtpy.png</background>
          <thumbs flipY="true" diffuse="Thumb_Mask.png">
          </thumbs>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png">
          </InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>yes</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="130,130" time="150">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="130,130" end="100,100" time="150">unfocus</thumbAnimation>
          <animation effect="rotatey" start="20" end="0" center="525,0" tween="back" ease="easeout" time="300" reversible="false">unfocus</animation>
          <animation effect="rotatey" start="0" end="20" center="525,0" tween="back" ease="easeout" time="300" reversible="false">focus</animation>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
          <itemWidth>32</itemWidth>
          <itemHeight>32</itemHeight>
          <IconXOff>12</IconXOff>
          <IconYOff>4</IconYOff>
        </control>
      </control>
    </control>
  </controls>
</window>