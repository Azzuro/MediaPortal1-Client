<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
    <controls>
        <control>
            <description>group element</description>
            <type>group</type>
            <visible>!Control.HasFocus(50)</visible>
            <animation effect="fade" time="250">WindowOpen</animation>
            <animation effect="fade" time="250">WindowClose</animation>
            <animation effect="slide" time="150" start="0,-300">WindowOpen</animation>
            <animation effect="slide" time="250" end="800,0">WindowClose</animation>
            <!--             <animation effect="rotatey" start="20" end="0" center="525,0" tween="back" ease="easeout" time="500" reversible="false">hidden</animation>
            <animation effect="rotatey" start="0" end="20" center="525,0" tween="back" ease="easeout" time="500" reversible="false">visible</animation> -->
            <control>
                <description>composite control consisting of a list control and a thumbnail panel</description>
                <type>facadeview</type>
                <id>50</id>
                <control>
                    <description>listcontrol</description>
                    <type>listcontrol</type>
                    <id>50</id>
                    <onleft>2</onleft>
                    <onright>2</onright>
                    <scrollOffset>2</scrollOffset>
                </control>
                <control>
                    <description>album view listcontrol</description>
                    <type>listcontrol</type>
                    <subtype>album</subtype>
                    <id>50</id>
                    <width>755</width>
                    <height>593</height>
                    <font>font13</font>
                    <font2>font13</font2>
                    <selectedColor>80000000</selectedColor>
                    <textcolor>ffffffff</textcolor>
                    <selectedColor2>ffffffff</selectedColor2>
                    <textcolor2>ffffffff</textcolor2>
                    <textXOff2>746</textXOff2>
                    <textXOff3>108</textXOff3>
                    <textYOff2>75</textYOff2>
                    <itemWidth>90</itemWidth>
                    <itemHeight>90</itemHeight>
                    <IconXOff>12</IconXOff>
                    <IconYOff>9</IconYOff>
                    <spaceBetweenItems>24</spaceBetweenItems>
                    <textureHeight>112</textureHeight>
                    <scrollOffset>1</scrollOffset>
                    <textureFocus>album_background_focus.png</textureFocus>
                    <textureNoFocus>album_background_nofocus.png</textureNoFocus>
                    <onleft>2</onleft>
                    <onright>50</onright>
                    <onup>2</onup>
                    <ondown>2</ondown>
                </control>        
                <control>
                    <description>Thumbnail Panel</description>
                    <type>thumbnailpanel</type>
                    <id>50</id>
                    <posX>462</posX>
                    <posY>121</posY>
                    <width>755</width>
                    <height>593</height>
                    <scrollOffset>0</scrollOffset>
                    <onleft>2</onleft>
                    <onright>50</onright>
                    <itemWidth>250</itemWidth>
                    <itemHeight>169</itemHeight>
                    <textureWidth>242</textureWidth>
                    <textureHeight>136</textureHeight>
                    <thumbWidth>235</thumbWidth>
                    <thumbHeight>130</thumbHeight>
                    <thumbPosX>5</thumbPosX>
                    <thumbPosY>5</thumbPosY>
                    <itemWidthBig>375</itemWidthBig>
                    <itemHeightBig>230</itemHeightBig>
                    <textureWidthBig>365</textureWidthBig>
                    <textureHeightBig>200</textureHeightBig>
                    <thumbWidthBig>360</thumbWidthBig>
                    <thumbHeightBig>195</thumbHeightBig>
                    <thumbPosXBig>5</thumbPosXBig>
                    <thumbPosYBig>5</thumbPosYBig>
                    <zoomXPixels>24</zoomXPixels>
                    <thumbZoom>false</thumbZoom>
                    <keepaspectratio>no</keepaspectratio>
                    <hideUnfocusTexture>no</hideUnfocusTexture>
                    <renderFocusText>no</renderFocusText>
                    <renderUnfocusText>yes</renderUnfocusText>
                </control>
            </control>
        </control>
    </controls>
</window>