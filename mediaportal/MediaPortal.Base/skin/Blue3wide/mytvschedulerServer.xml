<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>601</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:646</define>
  <define>#header.image:television_logo.png</define>
  <define>#header.hover:-</define>
  <controls>
    <import>common.time.xml</import>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <width>1280</width>
      <height>720</height>
    </control>
    <control>
      <type>image</type>
      <id>1</id>
      <posX>106</posX>
      <posY>12</posY>
      <texture>television_logo.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Scheduler Text Label</description>
      <type>label</type>
      <id>1</id>
      <posX>472</posX>
      <posY>60</posY>
      <align>left</align>
      <width>800</width>
      <label>646</label>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Number of Files Label</description>
      <type>label</type>
      <id>12</id>
      <posX>462</posX>
      <posY>682</posY>
      <label>#selectedindex/#itemcount #itemtype</label>
      <font>font10</font>
      <align>left</align>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>string.equals(#selectedindex)+string.equals(#itemcount)</visible>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <posX>71</posX>
      <posY>121</posY>
      <layout>StackLayout</layout>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <control>
        <type>sortbutton</type>
        <id>2</id>
        <label>620</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>17</onup>
        <ondown>11</ondown>
      </control>
      <control>
        <description>Toggle button series</description>
        <type>togglebutton</type>
        <id>11</id>
        <label>3019</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>2</onup>
        <ondown>6</ondown>
      </control>
      <control>
        <description>New button</description>
        <type>button</type>
        <id>6</id>
        <label>617</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>11</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>Cleanup</description>
        <type>button</type>
        <id>7</id>
        <label>624</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>6</onup>
        <ondown>8</ondown>
      </control>
      <control>
        <description>Priorities</description>
        <type>button</type>
        <id>8</id>
        <label>877</label>
        <hyperlink>607</hyperlink>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>7</onup>
        <ondown>9</ondown>
      </control>
      <control>
        <description>Conflicts</description>
        <type>button</type>
        <id>9</id>
        <label>892</label>
        <hyperlink>608</hyperlink>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>8</onup>
        <ondown>99</ondown>
      </control>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <control>
        <description>scheduler recordings listcontrol</description>
        <type>listcontrol</type>
        <id>10</id>
        <posX>462</posX>
        <posY>95</posY>
        <height>385</height>
        <width>755</width>
        <textXOff>10</textXOff>
        <textYOff>2</textYOff>
        <textXOff2>746</textXOff2>
        <textYOff2>32</textYOff2>
        <textXOff3>177</textXOff3>
        <textYOff3>32</textYOff3>
        <IconXOff>10</IconXOff>
        <PinIconXOff>80</PinIconXOff>
        <PinIconYOff>37</PinIconYOff>
        <PinIconWidth>35</PinIconWidth>
        <PinIconHeight>25</PinIconHeight>
        <spinWidth>46</spinWidth>
        <spinHeight>33</spinHeight>
        <spinPosX>1120</spinPosX>
        <spinPosY>650</spinPosY>
        <itemWidth>55</itemWidth>
        <itemHeight>55</itemHeight>
        <textureFocus>album_background_focus.png</textureFocus>
        <textureNoFocus>album_background_nofocus.png</textureNoFocus>
        <spaceBetweenItems>4</spaceBetweenItems>
        <keepaspectratio>yes</keepaspectratio>
        <textureHeight>65</textureHeight>
        <font2>font10</font2>
        <font3>font10</font3>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
      </control>
    </control>
    <control>
      <type>image</type>
      <posX>462</posX>
      <posY>475</posY>
      <width>80</width>
      <height>73</height>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <zoom>no</zoom>
      <texture>#selectedthumb</texture>
    </control>
    <control>
      <description>channel</description>
      <id>17</id>
      <type>fadelabel</type>
      <posX>552</posX>
      <posY>467</posY>
      <align>left</align>
      <width>674</width>
      <textcolor>FFB2D4F5</textcolor>
      <label>#TV.Scheduled.Channel</label>
      <font>font12</font>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Current program time</description>
      <id>14</id>
      <type>label</type>
      <posX>552</posX>
      <posY>495</posY>
      <width>280</width>
      <label>#TV.Scheduled.Time</label>
      <align>left</align>
      <font>font12</font>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Current program genre</description>
      <type>label</type>
      <id>0</id>
      <posX>1215</posX>
      <posY>495</posY>
      <width>380</width>
      <label>#TV.Scheduled.Genre</label>
      <align>right</align>
      <font>font12</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!string.equals(#TV.Scheduled.Genre,-)</visible>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>552</posX>
      <posY>523</posY>
      <width>664</width>
      <label>#TV.Scheduled.Title</label>
      <font>font12</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>462</posX>
      <posY>560</posY>
      <height>85</height>
      <width>755</width>
      <label>#TV.Scheduled.Description</label>
      <font>font11</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
  </controls>
</window>