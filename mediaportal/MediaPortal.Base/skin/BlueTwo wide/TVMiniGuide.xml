<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>3009</id>
  <defaultcontrol>35</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>background image</description>
      <type>image</type>
      <id>0</id>
      <texture>context_background.png</texture>
      <width>1020</width>
      <height>515</height>
      <posX>189</posX>
      <posY>133</posY>
    </control>
    <control>
      <description>exit button</description>
      <type>button</type>
      <id>34</id>
      <posX>1088</posX>
      <posY>160</posY>
      <width>58</width>
      <height>58</height>
      <textureFocus>dialogue_shutdown_focus.png</textureFocus>
      <textureNoFocus>dialogue_shutdown_nofocus.png</textureNoFocus>
      <label>-</label>
      <onleft>36</onleft>
      <onright>34</onright>
      <ondown>35</ondown>
      <onup>34</onup>
    </control>
    <control>
      <description>list to show channels</description>
      <type>listcontrol</type>
      <id>35</id>
      <posX>246</posX>
      <posY>230</posY>
      <width>825</width>
      <height>393</height>
      <textXOff>24</textXOff>
      <textYOff>24</textYOff>
      <textXOff2>805</textXOff2>
      <textYOff2>22</textYOff2>
      <textXOff3>75</textXOff3>
      <textYOff3>1</textYOff3>
      <IconXOff>22</IconXOff>
      <IconYOff>5</IconYOff>
      <PinIconXOff>845</PinIconXOff>
      <PinIconYOff>5</PinIconYOff>
      <spinPosX>1100</spinPosX>
      <spinPosY>592</spinPosY>
      <spinWidth>21</spinWidth>
      <spinHeight>21</spinHeight>
      <itemWidth>40</itemWidth>
      <itemHeight>40</itemHeight>
      <textureFocus>recording_background_focus.png</textureFocus>
      <textureNoFocus>recording_background_nofocus.png</textureNoFocus>
      <spaceBetweenItems>1</spaceBetweenItems>
      <textureHeight>50</textureHeight>
      <keepaspectratio>no</keepaspectratio>
      <font>font11</font>
      <font2>font11</font2>
      <font3>font12</font3>
      <remoteColor>ffffffff</remoteColor>
      <!-- used for selected channel -->
      <onleft>36</onleft>
      <onright>34</onright>
    </control>
    <control>
      <description>groups spin control</description>
      <type>spincontrol</type>
      <id>36</id>
      <posX>515</posX>
      <posY>170</posY>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <font>font13</font>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
      <ondown>35</ondown>
      <onup>36</onup>
      <onright>34</onright>
      <onleft>36</onleft>
    </control>
    <control>
      <description>GFX-Heading "menu" text label</description>
      <type>label</type>
      <id>5</id>
      <posX>288</posX>
      <posY>168</posY>
      <width>240</width>
      <label>10105</label>
      <font>font14</font>
      <textcolor>Black</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>      
    </control>
