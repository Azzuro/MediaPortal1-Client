<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>7890</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#selecteditem:#Play.Current.Lastfm.CurrentStream</define>
  <controls>
    <control>
      <type>image</type>
      <description>background image</description>
      <id>0</id>
      <width>720</width>
      <height>576</height>
    </control>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <import>common.music.play.fanart.xml</import>
    <control>
      <id>0</id>
      <description>album/artist/genre etc info label</description>
      <type>fadelabel</type>
      <posX>0</posX>
      <posY>68</posY>
      <width>720</width>
      <font>font17</font>
      <align>center</align>
      <label>#(L(179)): #Play.Current.Title\r#(L(171)) #Play.Current.Artist\r#(L(170)) #Play.Current.Album</label>
      <textcolor>ff6FAFE1</textcolor>
      <visible>player.hasaudio</visible>
    </control>
    <control>
      <type>image</type>
      <description>artist art image</description>
      <id>112</id>
      <posX>505</posX>
      <posY>115</posY>
      <width>174</width>
      <height>174</height>
      <texture mask="picturethumbmask.png">#Play.Current.ArtistThumb</texture>
      <centered>yes</centered>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <type>image</type>
      <description>artist art image</description>
      <id>0</id>
      <posX>505</posX>
      <posY>115</posY>
      <width>174</width>
      <height>174</height>
      <texture mask="picturethumbmask.png">picturethumbframe.png</texture>
      <centered>yes</centered>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <description>group element</description>
      <type>group</type>
      <layout>StackLayout</layout>
      <posX>39</posX>
      <posY>109</posY>
      <control>
        <description>start a stream</description>
        <type>button</type>
        <id>10</id>
        <posX>59</posX>
        <posY>109</posY>
        <label>34001</label>
        <onleft>10</onleft>
        <onright>123</onright>
        <onup>666</onup>
        <ondown>15</ondown>
      </control>
      <control>
        <description>choose artist</description>
        <type>button</type>
        <id>15</id>
        <label>Artists</label>
        <onleft>15</onleft>
        <onright>123</onright>
        <onup>10</onup>
        <ondown>20</ondown>
      </control>
      <control>
        <description>choose tag</description>
        <type>button</type>
        <id>20</id>
        <label>Tags</label>
        <onleft>20</onleft>
        <onright>123</onright>
        <onup>15</onup>
        <ondown>30</ondown>
      </control>
      <control>
        <description>choose friend</description>
        <type>button</type>
        <id>30</id>
        <label>Friends</label>
        <onleft>30</onleft>
        <onright>123</onright>
        <onup>20</onup>
        <ondown>35</ondown>
      </control>
      <control>
        <description>toggle submit mode</description>
        <type>togglebutton</type>
        <id>35</id>
        <label>34014</label>
        <onleft>35</onleft>
        <onright>123</onright>
        <onup>30</onup>
        <ondown>40</ondown>
        <visible>true</visible>
      </control>
      <control>
        <description>toggle discovery mode</description>
        <type>togglebutton</type>
        <id>40</id>
        <label>34013</label>
        <onleft>40</onleft>
        <onright>123</onright>
        <onup>35</onup>
        <ondown>10</ondown>
        <visible>false</visible>
      </control>
    </control>
    <control>
      <type>label</type>
      <description>stream label</description>
      <id>0</id>
      <posX>821</posX>
      <posY>76</posY>
      <width>399</width>
      <label>#Play.Current.Lastfm.CurrentStream</label>
      <font>font12</font>
      <align>left</align>
      <textcolor>ffffffff</textcolor>
    </control>
    <control>
      <type>label</type>
      <description>track progress label</description>
      <id>0</id>
      <posX>489</posX>
      <posY>372</posY>
      <label>#currentplaytime</label>
      <font>font14</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <type>image</type>
      <id>0</id>
      <posX>540</posX>
      <posY>374</posY>
      <width>101</width>
      <height>21</height>
      <texture>osd_progress_background.png</texture>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>1</id>
      <posX>540</posX>
      <posY>377</posY>
      <width>101</width>
      <offset>0</offset>
      <height>16</height>
      <innerheight>16</innerheight>
      <texturebg>-</texturebg>
      <lefttexture>-</lefttexture>
      <midtexture>osd_progress_mid.png</midtexture>
      <righttexture>-</righttexture>
      <label>#percentage</label>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <type>label</type>
      <description>track duration label</description>
      <id>0</id>
      <posX>691</posX>
      <posY>372</posY>
      <label>#trackduration</label>
      <font>font14</font>
      <align>right</align>
      <textcolor>ivory</textcolor>
      <visible>player.hasmedia</visible>
    </control>
    <control Style="BasicHomeFade">
      <description>background for hints/tags</description>
      <type>image</type>
      <id>0</id>
      <posX>28</posX>
      <posY>450</posY>
      <width>663</width>
      <height>104</height>
      <texture mask="osd_mask.png">black.png</texture>
      <colordiffuse>80ffffff</colordiffuse>
      <visible>control.hastext(444)|control.hastext(333)</visible>
    </control>
    <control>
      <type>label</type>
      <description>similar hint</description>
      <id>443</id>
      <posX>42</posX>
      <posY>461</posY>
      <label>34039</label>
      <font>font12</font>
      <align>left</align>
      <textcolor>ff6FAFE1</textcolor>
      <visible>control.hastext(444)</visible>
    </control>
    <control>
      <type>textbox</type>
      <description>Similar artists box</description>
      <id>444</id>
      <posX>42</posX>
      <posY>484</posY>
      <width>337</width>
      <height>72</height>
      <label>#Play.Current.Lastfm.SimilarArtists</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>label</type>
      <description>tag hint</description>
      <id>332</id>
      <posX>470</posX>
      <posY>461</posY>
      <label>34041</label>
      <font>font12</font>
      <align>left</align>
      <textcolor>ff6FAFE1</textcolor>
      <visible>control.hastext(333)</visible>
    </control>
    <control>
      <type>textbox</type>
      <description>Track tags box</description>
      <id>333</id>
      <posX>470</posX>
      <posY>484</posY>
      <width>209</width>
      <height>72</height>
      <label>#Play.Current.Lastfm.TrackTags</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>listcontrol</type>
      <description>current playlist control</description>
      <id>123</id>
      <posX>208</posX>
      <posY>112</posY>
      <height>355</height>
      <width>281</width>
      <onleft>10</onleft>
      <onright>123</onright>
      <font>font11</font>
      <font2>font11</font2>
      <font3>font11</font3>
      <textcolor2>ffffffff</textcolor2>
      <textXOff>24</textXOff>
      <textXOff2>42</textXOff2>
      <textXOff3>124</textXOff3>
      <textYOff>11</textYOff>
      <textYOff2>17</textYOff2>
      <textYOff3>17</textYOff3>
      <IconXOff>25</IconXOff>
      <textureFocus>lastfm_focus.png</textureFocus>
      <textureNoFocus>list_sub_nofocus.png</textureNoFocus>
      <keepaspectratio>no</keepaspectratio>
      <textureHeight>48</textureHeight>
      <itemHeight>40</itemHeight>
      <itemWidth>28</itemWidth>
      <spaceBetweenItems>1</spaceBetweenItems>
      <visible>player.hasmedia</visible>
    </control>
  </controls>
</window>
