    <!-- *** Image Facade *** -->
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!control.HasFocus(50)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <control>
        <description>Music facade</description>
        <type>facadeview</type>
        <id>50</id>
        <!-- *** Thumbnail *** -->
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>200</posX>
          <posY>108</posY>
          <width>500</width>
          <height>420</height>
          <itemWidthBig>165</itemWidthBig>
          <itemHeightBig>120</itemHeightBig>
          <textureWidthBig>155</textureWidthBig>
          <textureHeightBig>95</textureHeightBig>
          <thumbWidthBig>155</thumbWidthBig>
          <thumbHeightBig>95</thumbHeightBig>
          <thumbPosXBig>0</thumbPosXBig>
          <thumbPosYBig>0</thumbPosYBig>
          <textureMask>Picturethumbmask.png</textureMask>
          <frameNoFocus>Picturethumbframe.png</frameNoFocus>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <onleft>5</onleft>
          <onright>5</onright>
          <unfocusedAlpha>200</unfocusedAlpha>
          <enableFocusZoom>yes</enableFocusZoom>
          <zoomXPixels>50</zoomXPixels>
          <zoomYPixels>50</zoomYPixels>
          <hideUnfocusTexture>no</hideUnfocusTexture>
          <visible>string.equals(#WorldWeather.View,Image)+string.equals(#WorldWeather.ImageView,Thumbnail)+!string.equals(#WorldWeather.ImageSatellite, )</visible>
        </control>
        <!-- *** Image filmstrip *** -->
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <posX>130</posX>
          <posY>300</posY>
          <width>600</width>
          <height>200</height>
          <onup>5</onup>
          <onleft>5</onleft>
          <thumbWidth>120</thumbWidth>
          <thumbHeight>80</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <itemWidth>133</itemWidth>
          <itemHeight>184</itemHeight>
          <textureWidth>120</textureWidth>
          <textureHeight>80</textureHeight>
          <imageFolder>Picturethumbframe.png</imageFolder>
          <imageFolderFocus>Picturethumbframe.png</imageFolderFocus>
          <frame>Picturethumbframe.png</frame>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <textureMask>Picturethumbmask.png</textureMask>
          <thumbs mask="Picturethumbmask.png"></thumbs>
          <background>emtpy.png</background>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <keepaspectratio>no</keepaspectratio>
          <dimColor>80ffffff</dimColor>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>no</renderUnfocusText>
          <textYOff>1500</textYOff>
          <thumbAnimation effect="zoom" start="100,100" end="130,130" time="150">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="130,130" end="100,100" time="150">unfocus</thumbAnimation>
          <spinPosX>2640</spinPosX>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>170</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <thumbAnimation effect="zoom" start="100,100" end="230,230" time="70">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="230,230" end="100,100" time="70">unfocus</thumbAnimation>
          <visible>string.equals(#WorldWeather.View,Image)+string.equals(#WorldWeather.ImageView,Filmstrip)+!string.equals(#WorldWeather.ImageSatellite, )</visible>
        </control>
        <!-- *** Image coverflow *** -->
        <control>
          <description>Coverflow view</description>
          <type>coverflow</type>
          <id>50</id>
          <posX>0</posX>
          <posY>250</posY>
          <width>720</width>
          <height>576</height>
          <onup>5</onup>
          <onleft>5</onleft>
          <selectedCard>0</selectedCard>
          <cardWidth>240</cardWidth>
          <cardHeight>160</cardHeight>
          <angle>65</angle>
          <sideShift>200</sideShift>
          <sideGap>100</sideGap>
          <sideDepth>550</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>-20</selectedOffsetY>
          <speed>8</speed>
          <backgroundHeight>200</backgroundHeight>
          <backgroundWidth>720</backgroundWidth>
          <backgroundX>0</backgroundX>
          <backgroundY>376</backgroundY>
          <backgroundDiffuse>FF000000</backgroundDiffuse>
