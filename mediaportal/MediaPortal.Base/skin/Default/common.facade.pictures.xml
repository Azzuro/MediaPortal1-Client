<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      
      
      <animation effect="slide" time="150" start="0,-300">WindowOpen</animation>
      <animation effect="slide" time="150" end="800,0">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
          <unfocusedAlpha>160</unfocusedAlpha>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>45</posX>
          <posY>112</posY>
          <width>720</width>
          <height>438</height>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <textureWidthBig>110</textureWidthBig>
          <textureHeightBig>110</textureHeightBig>
          <itemWidthBig>130</itemWidthBig>
          <itemHeightBig>130</itemHeightBig>
          <textureWidth>88</textureWidth>
          <textureHeight>88</textureHeight>
          <itemWidth>110</itemWidth>
          <itemHeight>105</itemHeight>
          <thumbWidthBig>110</thumbWidthBig>
          <thumbHeightBig>110</thumbHeightBig>
          <thumbPosXBig>0</thumbPosXBig>
          <thumbPosYBig>0</thumbPosYBig>
          <thumbWidth>88</thumbWidth>
          <thumbHeight>88</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <zoomXPixels>40</zoomXPixels>
          <zoomYPixels>40</zoomYPixels>
          <hideUnfocusTexture>yes</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>no</renderUnfocusText>
          <showFrame>true</showFrame>
          <imageFolder>-</imageFolder>
          <imageFolderFocus>-</imageFolderFocus>
          <textureMask>Picturethumbmask.png</textureMask>
          <frameNoFocus>Picturethumbframe.png</frameNoFocus>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <keepaspectratio>no</keepaspectratio>
          <font>font10</font>
          <spinCanFocus>no</spinCanFocus>
          <dimColor>80ffffff</dimColor>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <colordiffuse>90ffffff</colordiffuse>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <posX>45</posX>
          <posY>204</posY>
          <width>731</width>
          <height>576</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <thumbWidth>122</thumbWidth>
          <thumbHeight>127</thumbHeight>
          <thumbPosX>2</thumbPosX>
          <thumbPosY>4</thumbPosY>
          <itemWidth>133</itemWidth>
          <itemHeight>184</itemHeight>
          <textureWidth>122</textureWidth>
          <textureHeight>127</textureHeight>
          <imageFolder>Picturethumbframe.png</imageFolder>
          <imageFolderFocus>Picturethumbframe.png</imageFolderFocus>
          <frame>Picturethumbframe.png</frame>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <textureMask>Picturethumbmask.png</textureMask>
          <thumbs mask="Picturethumbmask.png"></thumbs>
          <background>emtpy.png</background>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <keepaspectratio>no</keepaspectratio>
          <dimColor>80ffffff</dimColor>
          <thumbAnimation effect="zoom" start="100,100" end="130,130" time="150">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="130,130" end="100,100" time="150">unfocus</thumbAnimation>
        </control>
        <control>
          <description>Cover Flow view</description>
          <type>coverflow</type>
          <colordiffuse>90ffffff</colordiffuse>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>0</posX>
          <posY>160</posY>
          <width>720</width>
          <height>576</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <selectedCard>0</selectedCard>
          <cardWidth>270</cardWidth>
          <cardHeight>270</cardHeight>
          <angle>65</angle>
          <sideShift>200</sideShift>
          <sideGap>90</sideGap>
          <sideDepth>400</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>0</selectedOffsetY>
          <speed>8</speed>
          <backgroundHeight>200</backgroundHeight>
          <backgroundWidth>720</backgroundWidth>
          <backgroundX>0</backgroundX>
          <backgroundY>376</backgroundY>
          <backgroundDiffuse>FF000000</backgroundDiffuse>
          <background>background2.png</background>
          <showBackground>yes</showBackground>
          <foregroundHeight>720</foregroundHeight>
          <foregroundWidth>1280</foregroundWidth>
          <foregroundX>0</foregroundX>
          <foregroundY>0</foregroundY>
          <foregroundDiffuse>FF000000</foregroundDiffuse>
          <foreground>coverflow_foreground.png</foreground>
          <showForeground>no</showForeground>
          <showFrame>yes</showFrame>
          <frame>Picture_cover2.png</frame>
          <frameFocus>Picture_cover2.png</frameFocus>
          <frameWidth>270</frameWidth>
          <frameHeight>270</frameHeight>
          <spinSpeed>8</spinSpeed>
          <unfocusedAlpha>FF</unfocusedAlpha>
          <folderPrefix>[</folderPrefix>
          <folderSuffix>]</folderSuffix>
          <font1>font13</font1>
          <font2>font10</font2>
          <label1></label1>
          <label2></label2>
          <textColor>FFFFFFFF</textColor>
          <remoteColor>FFFF0000</remoteColor>
          <playedColor>FFA0D0FF</playedColor>
          <downloadColor>FF00FF00</downloadColor>
          <selectedColor>FFFFFFFF</selectedColor>
          <shadowAngle>45</shadowAngle>
          <shadowDistance>0</shadowDistance>
          <shadowColor>FF000000</shadowColor>
          <label1YOff>405</label1YOff>
          <label2YOff>430</label2YOff>
          <pageSize>5</pageSize>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>285</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <cards mask="Picturethumbmask.png">Picturethumbmask.png</cards>
          <subitems>
            <subitem>
              <![CDATA[                 <controls>
              <control>
                <description>background</description>
                <type>image</type>
                <posX>0</posX>
                <posY>0</posY>
                <width>270</width>
                <height>270</height>
                <texture flipY="yes" diffuse="Thumb_Mask.png" mask="Picturethumbmask.png">Picturethumbmask.png</texture>
              </control>
              <control>
                <description>Picture Details</description>
                <type>label</type>
                <posX>20</posX>
                <posY>10</posY>
                <width>270</width>
                <label>Picture Details</label>
                <font>font10</font>
                <textcolor>gainsboro</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>General Info</description>
                <type>label</type>
                <posX>0</posX>
                <posY>60</posY>
                <width>270</width>
                <label>#selecteditem</label>
                <font>font12</font>
                <align>center</align>
                <textcolor>gainsboro</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>Picture size</description>
                <type>label</type>
                <posX>0</posX>
                <posY>90</posY>
                <width>270</width>
                <label>#selecteditem2</label>
                <font>font12</font>
                <align>center</align>
                <textcolor>gainsboro</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
            </controls>]]></subitem>
        </subitems>
      </control>
    </control>
  </control>
</controls>
</window>
