<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>600</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>0</id>
      <width>720</width>
      <height>576</height>
      <colordiffuse>ffffffff</colordiffuse>
    </control>
    <control>
      <description>Date Time</description>
      <type>label</type>
      <id>0</id>
      <posX>699</posX>
      <posY>9</posY>
      <label>#date | #time</label>
      <align>right</align>
      <font>font13</font>
      <textcolor>ffa9d0f7</textcolor>
    </control>
    <control>
      <description>Guide Group Label</description>
      <type>label</type>
      <id>11</id>
      <posX>33</posX>
      <posY>9</posY>
      <width>393</width>
      <font>font14</font>
      <align>left</align>
      <label>#TV.Guide.Group</label>
    </control>
    <control>
      <description>TVGUIDE Panel</description>
      <type>image</type>
      <id>2</id>
      <posX>82</posX>
      <posY>89</posY>
      <width>617</width>
      <height>340</height>
      <colordiffuse>ffffffff</colordiffuse>
      <texture>-</texture>
    </control>
    <control>
      <description>Single Channel Label</description>
      <type>label</type>
      <id>32</id>
      <posX>240</posX>
      <posY>32</posY>
      <font>font14</font>
      <align>left</align>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>Single Channel Image</description>
      <type>image</type>
      <id>33</id>
      <posX>200</posX>
      <posY>25</posY>
      <width>30</width>
      <height>30</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>defaultVideoBig.png</texture>
    </control>
    <!-- Timeheader background must be dedfined before Timeheader label template -->
    <!-- b/c Timeheader is on the background and will be rendered first -->
    <control>
      <description>time header background image template</description>
      <type>image</type>
      <id>90</id>
      <posX>191</posX>
      <posY>36</posY>
      <width>114</width>
      <height>28</height>
      <texture>button_small_settings_nofocus.png</texture>
    </control>
    <control>
      <description>time header label template</description>
      <type>label</type>
      <id>40</id>
      <posX>191</posX>
      <posY>36</posY>
      <font>font13</font>
    </control>
    <control>
      <description>day spin control</description>
      <type>spincontrol</type>
      <id>6</id>
      <posX>21</posX>
      <posY>40</posY>
      <width>21</width>
      <height>21</height>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <onup>21</onup>
      <onleft>60</onleft>
      <onright>8</onright>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
    </control>
    <control>
      <description>time interval spin control</description>
      <type>spincontrol</type>
      <id>8</id>
      <posX>139</posX>
      <posY>40</posY>
      <width>21</width>
      <height>21</height>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
      <onup>21</onup>
      <onleft>6</onleft>
      <onright>8</onright>
    </control>
    <control>
      <description>Channel template</description>
      <type>image</type>
      <id>20</id>
      <posX>44</posX>
      <posY>64</posY>
      <width>147</width>
      <height>34</height>
    </control>
    <control>
      <description>Channel image template</description>
      <type>image</type>
      <id>7</id>
      <posX>1</posX>
      <posY>1</posY>
      <width>29</width>
      <height>29</height>
      <zoom>no</zoom>
      <keepaspectratio>yes</keepaspectratio>
      <texture>defaultVideoBig.png</texture>
    </control>
    <control>
      <description>channel Label template</description>
      <type>label</type>
      <id>18</id>
      <posX>36</posX>
      <posY>4</posY>
      <width>64</width>
      <height>14</height>
      <font>font12</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control Style="NoShadow">
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>196</posX>
      <posY>418</posY>
      <width>230</width>
      <font>font12</font>
      <label>#TV.Guide.CompositeTitle</label>
    </control>
    <control Style="NoShadow">
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>688</posX>
      <posY>418</posY>
      <align>right</align>
      <font>font12</font>
      <textcolor>ffa9d0f7</textcolor>
      <label>#TV.Guide.Time</label>
    </control>
    <control Style="NoShadow">
      <description>Current program genre</description>
      <type>label</type>
      <id>27</id>
      <posX>688</posX>
      <posY>546</posY>
      <width>199</width>
      <font>font12</font>
      <align>right</align>
      <textcolor>ffa9d0f7</textcolor>
      <label>#TV.Guide.Genre</label>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>196</posX>
      <posY>449</posY>
      <width>455</width>
      <height>89</height>
      <font>font11</font>
      <label>#TV.Guide.Description</label>
      <shadowAngle>0</shadowAngle>
      <shadowDistance>0</shadowDistance>
      <shadowColor />
    </control>
    <control Style="NoShadow">
      <description>template for program title light area</description>
      <type>label</type>
      <id>24</id>
      <visible>no</visible>
      <label>-</label>
      <font>font12</font>
      <textcolor>ccffffff</textcolor>
      <posX>5</posX>
      <posY>4</posY>
    </control>
    <control>
      <description>vertical timeline</description>
      <type>image</type>
      <id>25</id>
      <posX>29</posX>
      <posY>62</posY>
      <height>354</height>
      <colordiffuse>ffffffff</colordiffuse>
      <texture>tvguide_slider.png</texture>
    </control>
    <control Style="NoShadow">
      <description>template for program title darkarea</description>
      <type>label</type>
      <visible>no</visible>
      <id>26</id>
      <label>-</label>
      <font>font12</font>
      <textcolor>ffffcc00</textcolor>
      <posX>5</posX>
      <posY>4</posY>
    </control>
    <control>
      <description>TV background</description>
      <type>image</type>
      <id>98</id>
      <posX>43</posX>
      <posY>442</posY>
      <width>121</width>
      <height>100</height>
      <texture>LCD.png</texture>
      <visible>Player.HasVideo</visible>
    </control>
    <control>
      <description>TV preview window</description>
      <type>videowindow</type>
      <id>99</id>
      <posX>48</posX>
      <posY>446</posY>
      <width>114</width>
      <height>92</height>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <action>18</action>
      <textureFocus>tv_green_border.png</textureFocus>
      <visible>Player.HasVideo</visible>
    </control>
    <control>
      <description>record pin</description>
      <type>image</type>
      <id>31</id>
      <posX>655</posX>
      <posY>449</posY>
      <align>left</align>
      <texture>tvguide_record_button.png</texture>
      <visible>no</visible>
    </control>
    <control>
      <description>TvGroup button</description>
      <type>button</type>
      <id>100</id>
      <posX>18</posX>
      <posY>66</posY>
      <width>25</width>
      <height>340</height>
      <textureFocus>small_vbutton_focus.png</textureFocus>
      <textureNoFocus>small_vbutton_nofocus.png</textureNoFocus>
      <label />
      <onleft>60</onleft>
      <onright>6</onright>
      <onup>6</onup>
      <ondown>6</ondown>
      <animation effect="zoom" start="100,100" end="100,100" time="0">focus</animation>
      <animation effect="zoom" start="100,100" end="100,100" time="0">unfocus</animation>
    </control>
    <control>
      <description>TvGroup button label</description>
      <type>label</type>
      <id>0</id>
      <posX>39</posX>
      <posY>72</posY>
      <label>#TV.Guide.ChangeGroup</label>
      <font>font12</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="rotate" end="-90" time="0">WindowOpen</animation>
    </control>
  </controls>
</window>
