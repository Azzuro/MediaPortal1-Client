<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>3007</id>
  <defaultcontrol>213</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>background botton</description>
      <id>1</id>
      <type>image</type>
      <posX>0</posX>
      <posY>465</posY>
      <height>136</height>
      <width>731</width>
      <texture>blue.png</texture>
      <colordiffuse>eeffffff</colordiffuse>
    </control>
    <control>
      <description>Time Info</description>
      <type>label</type>
      <id>110</id>
      <posX>703</posX>
      <posY>479</posY>
      <label>#time</label>
      <align>right</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>image</type>
      <id>10</id>
      <posX>22</posX>
      <posY>480</posY>
      <width>56</width>
      <height>80</height>
      <texture>#TV.View.thumb</texture>
      <centered>yes</centered>
      <keepaspectratio>yes</keepaspectratio>
    </control>
    <control>
      <description>Current channel</description>
      <type>label</type>
      <id>35</id>
      <posX>527</posX>
      <posY>508</posY>
      <width>185</width>
      <label>-</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>image</type>
      <description>progressbar bg</description>
      <id>1</id>
      <posX>101</posX>
      <posY>512</posY>
      <width>405</width>
      <height>21</height>
      <texture>osd_progress_background.png</texture>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>20</id>
      <posX>101</posX>
      <posY>515</posY>
      <width>405</width>
      <offset>0</offset>
      <height>16</height>
      <innerheight>20</innerheight>
      <lefttexture>-</lefttexture>
      <midtexture>osd_progress_mid.png</midtexture>
      <righttexture>-</righttexture>
      <label>#TV.View.Percentage</label>
      <visible>yes</visible>
    </control>
    <control>
      <description>Start Time Info</description>
      <type>label</type>
      <id>101</id>
      <posX>101</posX>
      <posY>480</posY>
      <label>#TV.View.start</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <description>On tv NOW</description>
      <id>36</id>
      <type>textbox</type>
      <posX>180</posX>
      <posY>479</posY>
      <width>281</width>
      <height>24</height>
      <label>#TV.View.title</label>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <description>End Time Info</description>
      <type>label</type>
      <id>102</id>
      <posX>101</posX>
      <posY>536</posY>
      <label>#TV.View.Stop</label>
      <align>left</align>
      <textcolor>white</textcolor>
      <font>font14</font>
      <shadowAngle>0</shadowAngle>
      <shadowDistance>0</shadowDistance>
      <shadowColor />
    </control>
    <control>
      <description>On tv NEXT</description>
      <id>37</id>
      <type>textbox</type>
      <posX>180</posX>
      <posY>534</posY>
      <height>24</height>
      <width>281</width>
      <align>left</align>
      <font>font14</font>
      <textcolor>white</textcolor>
      <shadowAngle>0</shadowAngle>
      <shadowDistance>0</shadowDistance>
      <shadowColor />
    </control>
    <control>
      <description>rec logo</description>
      <type>image</type>
      <id>39</id>
      <posX>506</posX>
      <posY>510</posY>
      <width>28</width>
      <height>40</height>
      <texture>tv_is_recording.png</texture>
      <animation effect="zoom" start="100,100" end="110,110" time="70" condition="true" pulse="true">conditional</animation>
    </control>
    <control>
      <description>divider</description>
      <type>image</type>
      <id>1</id>
      <posX>517</posX>
      <posY>480</posY>
      <height>88</height>
      <texture>slider_vertical.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
    </control>
    <control>
      <description>Zap to channel no</description>
      <type>label</type>
      <id>38</id>
      <posX>28</posX>
      <posY>24</posY>
      <label>-</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>lime</textcolor>
      <animation effect="zoom" start="100,100" end="160,160" time="10">visiblechange</animation>
    </control>
  </controls>
</window>
