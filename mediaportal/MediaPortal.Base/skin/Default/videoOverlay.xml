<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>3000</id>
  <defaultcontrol>2</defaultcontrol>
  <controls>
    <control>
      <description>rectangle image</description>
      <type>image</type>
      <id>0</id>
      <posX>16</posX>
      <posY>437</posY>
      <width>185</width>
      <height>144</height>
      <texture mask="picturethumbmask.png">blue.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
    </control>
    <control>
      <description>play time label</description>
      <type>label</type>
      <id>2</id>
      <posX>155</posX>
      <posY>496</posY>
      <label>#currentplaytime</label>
      <align>left</align>
      <font>font12</font>
      <textcolor>80ffffff</textcolor>
    </control>
    <control>
      <description>play logo</description>
      <type>image</type>
      <id>3</id>
      <posX>155</posX>
      <posY>456</posY>
      <width>29</width>
      <height>29</height>
      <texture>play_enabled.png</texture>
      <visible>!player.paused+!player.forwarding+!player.rewinding</visible>
    </control>
    <control>
      <description>Pause logo</description>
      <type>image</type>
      <id>4</id>
      <posX>155</posX>
      <posY>456</posY>
      <width>29</width>
      <height>29</height>
      <texture>pause_enabled.png</texture>
      <visible>player.paused</visible>
    </control>
    <control>
      <description>FF logo</description>
      <type>image</type>
      <id>7</id>
      <posX>155</posX>
      <posY>456</posY>
      <width>29</width>
      <height>29</height>
      <texture>fastforward_enabled.png</texture>
      <visible>player.forwarding</visible>
    </control>
    <control>
      <description>RW logo</description>
      <type>image</type>
      <id>8</id>
      <posX>155</posX>
      <posY>456</posY>
      <width>29</width>
      <height>29</height>
      <texture>rew_enabled.png</texture>
      <visible>player.rewinding</visible>
    </control>
    <control>
      <description>scrolling info label Video</description>
      <type>fadelabel</type>
      <id>5</id>
      <posX>24</posX>
      <posY>544</posY>
      <width>171</width>
      <height>29</height>
      <font>font12</font>
      <label>#Play.Current.Title\r#Play.Current.Genre\r#Play.Current.Year\r#Play.Current.Director</label>
      <textcolor>EFFFFFFF</textcolor>
    </control>
    <control>
      <description>video rectangle</description>
      <type>image</type>
      <id>0</id>
      <posX>24</posX>
      <posY>442</posY>
      <width>122</width>
      <height>100</height>
      <texture>LCD.png</texture>
    </control>
    <control>
      <description>video preview window</description>
      <type>videowindow</type>
      <id>1</id>
      <posX>28</posX>
      <posY>446</posY>
      <width>114</width>
      <height>92</height>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <action>18</action>
      <textureFocus>tv_green_border.png</textureFocus>
    </control>
  </controls>
</window>
