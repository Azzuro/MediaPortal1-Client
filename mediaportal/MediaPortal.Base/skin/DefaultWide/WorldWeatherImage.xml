<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <!-- *** Location *** -->
    <control>
      <description>Location label</description>
      <type>label</type>
      <id>0</id>
      <posX>60</posX>
      <posY>27</posY>
      <width>600</width>
      <label>#(L(8))  #WorldWeather.TranslationSatelliteImage</label>
      <font>font16</font>
      <align>left</align>
      <textcolor>white</textcolor>
      <visible>string.equals(#WorldWeather.View,Image)</visible>
    </control>
    <!-- *** Satellite Image *** -->
    <control>
      <description>Satellite image</description>
      <type>image</type>
      <id>9999</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageSatellite</texture>
      <visible>string.equals(#WorldWeather.View,Image)+string.equals(#WorldWeather.ImageView,Flat)+[control.hasfocus(10)|control.hasfocus(2)|control.hasfocus(3)|control.hasfocus(4)|control.hasfocus(5)|control.hasfocus(4)|control.hasfocus(9)]</visible>
    </control>
    <!-- *** Satellite Precipitation Image *** -->
    <control>
      <description>Satellite image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImagePrecipitation</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(11)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite Temperature Image *** -->
    <control>
      <description>Satellite image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageTemperature</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(12)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite Wind Image *** -->
    <control>
      <description>Satellite image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageWind</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(13)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite UV-INDEX Image *** -->
    <control>
      <description>Satellite UV-Index image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageUVIndex</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(14)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite POLLEN Image *** -->
    <control>
      <description>Satellite Pollen image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImagePollenCount</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(15)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite Suntime Image *** -->
    <control>
      <description>Satellite Suntime image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageSuntime</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(16)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite World Image *** -->
    <control>
      <description>Satellite World image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageWorld</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(17)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Satellite Webcam Image *** -->
    <control>
      <description>Satellite Webcam image</description>
      <type>image</type>
      <id>0</id>
      <posX>350</posX>
      <posY>120</posY>
      <width>980</width>
      <height>542</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#WorldWeather.ImageWebcam</texture>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(18)+string.equals(#WorldWeather.ImageView,Flat)</visible>
    </control>
    <!-- *** Image Facade *** -->
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!control.HasFocus(50)</visible>
      <control>
        <description>Weather facade</description>
        <type>facadeview</type>
        <id>50</id>
        <!-- *** Image filmstrip *** -->
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <posX>150</posX>
          <posY>400</posY>
          <width>1100</width>
          <height>720</height>
          <onup>5</onup>
          <onleft>5</onleft>
          <itemWidth>200</itemWidth>
          <itemHeight>168</itemHeight>
          <textureWidth>192</textureWidth>
          <textureHeight>130</textureHeight>
          <thumbWidth>192</thumbWidth>
          <thumbHeight>130</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <keepaspectratio>no</keepaspectratio>
          <imageFolder>Picturethumbframe.png</imageFolder>
          <imageFolderFocus>Picturethumbframe.png</imageFolderFocus>
          <frame>Picturethumbframe.png</frame>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <textureMask>Picturethumbmask.png</textureMask>
          <thumbs mask="Picturethumbmask.png"></thumbs>
          <background>emtpy.png</background>
          <InfoImage></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <keepaspectratio>no</keepaspectratio>
          <dimColor>80ffffff</dimColor>
          <spinPosX>2640</spinPosX>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>250</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <thumbAnimation effect="zoom" start="100,100" end="230,230" time="70">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="230,230" end="100,100" time="70">unfocus</thumbAnimation>
          <visible>string.equals(#WorldWeather.View,Image)+string.equals(#WorldWeather.ImageView,Filmstrip)+!string.equals(#WorldWeather.ImageSatellite, )</visible>
        </control>
        <!-- *** Image coverflow *** -->
        <control>
          <description>Coverflow view</description>
          <type>coverflow</type>
          <id>50</id>
          <posX>0</posX>
          <posY>300</posY>
          <width>1280</width>
          <height>720</height>
          <onleft>5</onleft>
          <onright>5</onright>
          <onup>5</onup>
          <ondown>5</ondown>
          <selectedCard>0</selectedCard>
          <cardWidth>450</cardWidth>
          <cardHeight>300</cardHeight>
          <angle>65</angle>
          <sideShift>500</sideShift>
          <sideGap>100</sideGap>
          <sideDepth>700</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>-20</selectedOffsetY>
          <speed>8</speed>
          <backgroundColor>000000FF</backgroundColor>
          <backgroundHeight>100</backgroundHeight>
          <backgroundWidth>1366</backgroundWidth>
          <backgroundX>0</backgroundX>
          <backgroundY>470</backgroundY>
          <backgroundDiffuse>60000000</backgroundDiffuse>
          <background></background>
          <showBackground>no</showBackground>
          <showFrame>yes</showFrame>
          <frame>Picture_cover2.png</frame>
          <frameFocus>Picture_cover2.png</frameFocus>
          <frameWidth>450</frameWidth>
          <frameHeight>300</frameHeight>
          <spinSpeed>8</spinSpeed>
          <unfocusedAlpha>FF</unfocusedAlpha>
          <folderPrefix></folderPrefix>
          <folderSuffix></folderSuffix>
          <font1>font14</font1>
          <font2>font10</font2>
          <label1></label1>
          <label2></label2>
          <keepaspectratio>no</keepaspectratio>
          <textColor>FFFFFFFF</textColor>
          <remoteColor>FFFF0000</remoteColor>
          <playedColor>FFA0D0FF</playedColor>
          <downloadColor>FF00FF00</downloadColor>
          <selectedColor>FFFFFFFF</selectedColor>
          <shadowDistance>1</shadowDistance>
          <label1YOff>-20</label1YOff>
          <label2YOff>390</label2YOff>
          <pageSize>100</pageSize>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>330</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <cards mask="Picturethumbmask.png">Picturethumbmask.png</cards>
          <visible>string.equals(#WorldWeather.View,Image)+string.equals(#WorldWeather.ImageView,Coverflow)+!string.equals(#WorldWeather.ImageSatellite, )</visible>
        </control>
      </control>
    </control>
    <control>
      <description>Label for filmstrip view</description>
      <type>label</type>
      <id>0</id>
      <posX>0</posX>
      <posY>200</posY>
      <width>1280</width>
      <label>#selecteditem</label>
      <font>font10</font>
      <align>center</align>
      <textcolor>white</textcolor>
      <visible>string.equals(#WorldWeather.View,Image)+control.hasfocus(50)</visible>
    </control>
  </controls>
</window>
