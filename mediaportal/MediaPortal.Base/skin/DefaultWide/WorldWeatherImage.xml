          <id>50</id>
          <posX>0</posX>
          <posY>300</posY>
          <width>1280</width>
          <height>720</height>
          <onleft>5</onleft>
          <onright>5</onright>
          <onup>5</onup>
          <ondown>5</ondown>
          <selectedCard>0</selectedCard>
          <cardWidth>450</cardWidth>
          <cardHeight>300</cardHeight>
          <angle>65</angle>
          <sideShift>500</sideShift>
          <sideGap>100</sideGap>
          <sideDepth>700</sideDepth>
          <offsetY>0</offsetY>
