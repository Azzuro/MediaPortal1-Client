<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <!-- *** Starry Sky *** -->
    <control>
      <description>Location label</description>
      <type>label</type>
      <id>0</id>
      <posX>60</posX>
      <posY>27</posY>
      <width>600</width>
      <label>#(L(8))  #WorldWeather.TranslationStarrySky</label>
      <font>font16</font>
      <align>left</align>
      <textcolor>white</textcolor>
      <visible>string.equals(#WorldWeather.View,StarrySky)</visible>
    </control>
    <control>
      <description>Starry sky image</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>80</posY>
      <width>640</width>
      <height>640</height>
      <texture>http://www.astronomie.de/Images/Himmelaktuell/obssky1.gif</texture>
      <visible>string.equals(#WorldWeather.View,StarrySky)</visible>
    </control>
    <control>
      <description>Starry sky image</description>
      <type>image</type>
      <id>0</id>
      <posX>640</posX>
      <posY>80</posY>
      <width>640</width>
      <height>640</height>
      <texture>http://www.astronomie.de/Images/Himmelaktuell/obssky1b.gif</texture>
      <visible>string.equals(#WorldWeather.View,StarrySky)</visible>
    </control>
  </controls>
</window>
