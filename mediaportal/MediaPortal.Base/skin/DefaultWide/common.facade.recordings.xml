        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <posX>462</posX>
          <posY>110</posY>
          <width>755</width>
          <height>380</height>
          <scrollOffset>2</scrollOffset>
          <textXOff>10</textXOff>
          <textYOff>2</textYOff>
