<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>Logos group</description>
      <type>group</type>
      <posX>71</posX>
      <posY>655</posY>
      <layout>StackLayout(0, Horizontal, true)</layout>
      <control>
        <description>Video codec (normal osd)</description>
        <type>image</type>
        <id>0</id>
        <posX>135</posX>
        <posY>655</posY>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.VideoCodec.Texture.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Video resolution</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.VideoResolution.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Aspect ratio</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.AspectRatio.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Audio codec (normal osd)</description>
        <type>image</type>
        <id>0</id>
        <posX>70</posX>
        <posY>655</posY>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.AudioCodec.Texture.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Audio channels</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.AudioChannels.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Subtitles</description>
        <type>image</type>
        <id>0</id>
        <posX>200</posX>
        <posY>655</posY>
        <width>69</width>
        <height>37</height>
        <visible>string.equals(#Play.Current.HasSubtitles, True)</visible>
        <texture>logos\subtitles.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Certification (MPAA) Logo</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>37</height>
        <texture>logos\#Play.Current.MPAARating.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
    </control>
  </controls>
</window>
