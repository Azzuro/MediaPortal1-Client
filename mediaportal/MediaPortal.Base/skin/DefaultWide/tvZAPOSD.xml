<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>3007</id>
  <defaultcontrol>213</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>background botton</description>
      <id>1</id>
      <type>image</type>
      <posX>0</posX>
      <posY>582</posY>
      <height>170</height>
      <width>1300</width>
      <texture>blue.png</texture>
      <colordiffuse>eeffffff</colordiffuse>
    </control>
    <control>
      <description>Time Info</description>
      <type>label</type>
      <id>110</id>
      <posX>1250</posX>
      <posY>599</posY>
      <label>#time</label>
      <align>right</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>image</type>
      <id>10</id>
      <posX>40</posX>
      <posY>600</posY>
      <width>100</width>
      <height>100</height>
      <texture>#TV.View.thumb</texture>
      <centered>yes</centered>
      <keepaspectratio>yes</keepaspectratio>
    </control>
    <control>
      <description>Current channel</description>
      <type>label</type>
      <id>35</id>
      <posX>937</posX>
      <posY>635</posY>
      <width>330</width>
      <label>-</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>image</type>
      <description>progressbar bg</description>
      <id>1</id>
      <posX>180</posX>
      <posY>640</posY>
      <width>720</width>
      <height>27</height>
      <texture>osd_progress_background.png</texture>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>20</id>
      <posX>180</posX>
      <posY>644</posY>
      <width>720</width>
      <offset>0</offset>
      <height>20</height>
      <innerheight>20</innerheight>
      <lefttexture>-</lefttexture>
      <midtexture>osd_progress_mid_green.png</midtexture>
      <righttexture>-</righttexture>
      <label>#TV.View.Percentage</label>
      <visible>yes</visible>
    </control>
    <control>
      <description>Start Time Info</description>
      <type>label</type>
      <id>101</id>
      <posX>180</posX>
      <posY>601</posY>
      <label>#TV.View.start</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <description>On tv NOW</description>
      <id>36</id>
      <type>textbox</type>
      <posX>320</posX>
      <posY>599</posY>
      <width>500</width>
      <height>30</height>
      <label>#TV.View.title</label>
      <font>font14</font>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <description>End Time Info</description>
      <type>label</type>
      <id>102</id>
      <posX>180</posX>
      <posY>670</posY>
      <label>#TV.View.Stop</label>
      <align>left</align>
      <textcolor>ivory</textcolor>
      <font>font14</font>
      <shadowAngle>0</shadowAngle>
      <shadowDistance>0</shadowDistance>
      <shadowColor></shadowColor>
    </control>
    <control>
      <description>On tv NEXT</description>
      <id>37</id>
      <type>textbox</type>
      <posX>320</posX>
      <posY>668</posY>
      <height>30</height>
      <width>500</width>
      <align>left</align>
      <font>font14</font>
      <textcolor>ivory</textcolor>
      <shadowAngle>0</shadowAngle>
      <shadowDistance>0</shadowDistance>
      <shadowColor></shadowColor>
    </control>
    <control>
      <description>rec logo</description>
      <type>image</type>
      <id>39</id>
      <posX>900</posX>
      <posY>638</posY>
      <width>50</width>
      <height>50</height>
      <texture>tv_is_recording.png</texture>
      <animation effect="zoom" start="100,100" end="110,110" time="70" condition="true" pulse="true">conditional</animation>
    </control>
    <control>
      <description>divider</description>
      <type>image</type>
      <id>1</id>
      <posX>920</posX>
      <posY>600</posY>
      <height>110</height>
      <texture>slider_vertical.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
    </control>
    <control>
      <description>Zap to channel no</description>
      <type>label</type>
      <id>38</id>
      <posX>50</posX>
      <posY>30</posY>
      <label>-</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>lime</textcolor>
      <animation effect="zoom" start="100,100" end="160,160" time="10" >visiblechange</animation>
    </control>
  </controls>
</window>
