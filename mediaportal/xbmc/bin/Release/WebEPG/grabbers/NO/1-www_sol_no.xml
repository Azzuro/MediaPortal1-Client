<!-- Formating of Listing -->
  <section name="Listing">
    <entry name="BaseURL">http://www.sol.no/underholdning/tv/guiden/</entry>
    <entry name="SearchURL">index.cgi?StartTime=0500&amp;EndTime=2900&amp;Date=[YYYY]-[MM]-[DD]&amp;Categories=*&amp;Channels=[ID]</entry>

    <entry name="Start"/>

<!-- Template of the Listing information -->
    <entry name="Template">
&lt;td>&lt;/td>
&lt;/tr>&lt;tr>
&lt;td>&lt;/td>
&lt;td>&lt;#START>&lt;br>&lt;#END>&lt;br>&lt;/td>
&lt;td>&lt;#GENRE>&lt;/td>
&lt;td>&lt;#TITLE>&lt;br>&lt;#DESCRIPTION>&lt;/td>
    </entry>

    <entry name="SearchRegex">yes</entry>
    <entry name="SearchRemove">yes</entry>
    <entry name="SearchRepeat">\(R\)</entry>
    <entry name="SearchSubtitles">\([t]+\)</entry>
    <entry name="SearchEpNum"> \([0-9]{1,4}[\):]</entry>
    <entry name="SearchEpTotal">\x06[0-9]{1,4}\)</entry>


    <entry name="SubListingLink">tvguiden</entry>
  </section>

