<window>
	<id>3007</id> 
	<defaultcontrol>213</defaultcontrol> 
	<allowoverlay>no</allowoverlay>
<controls>
	<control>
		<description>background</description> 
		<type>image</type> 
		<id>0</id> 
		<posX>10</posX> 
		<posY>490</posY>
		<texture>osd_bg.png</texture>		
		<visible>yes</visible> 
	</control>
	<control>
		<description>progress background</description> 
		<type>image</type> 
		<id>0</id> 
		<posX>350</posX> 
		<posY>524</posY>
		<width>375</width>
		<height>22</height>
		<texture>osd_progress_background.png</texture>		
		<visible>yes</visible> 
	</control>
	<control>
		<description>Progress Bar</description> 
		<type>progress</type> 
		<id>1</id> 
		<posX>339</posX> 
		<posY>525</posY> 
		<width>400</width>
		<height>20</height>
		<texturebg>osd_progress_back.png</texturebg> 
		<lefttexture>osd_progress_left.png</lefttexture> 
		<midtexture>osd_progress_mid.png</midtexture> 
		<righttexture>osd_progress_right.png</righttexture> 
		<visible>yes</visible> 
		<label>#TV.View.Percentage</label>
	</control>
	<control>
		<description>Start Time Info</description> 
		<type>label</type> 
