<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:c="clr-namespace:System.Collections;assembly=mscorlib" 
                    xmlns:l="clr-namespace:MCEControls" 
                    >

    <!-- Control background -->
    <SolidColorBrush x:Key="PageGlassyBackground" Color="#569dc2"/>

    <!-- Focus background -->
    <LinearGradientBrush x:Key="GlassyBackground" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#60FFFFFF" Offset="0" />
            <GradientStop Color="#30FFFFFF" Offset="0.4" />
            <GradientStop Color="#20FFFFFF" Offset="0.7" />
            <GradientStop Color="#40FFFFFF" Offset="1" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="GlassyBorder" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#E0FFFFFF" Offset="0" />
            <GradientStop Color="#80FFFFFF" Offset="0.4" />
            <GradientStop Color="#70FFFFFF" Offset="0.8" />
            <GradientStop Color="#90FFFFFF" Offset="1" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>


    <!--=================================================================
        SoftKey
    ==================================================================-->

    <Style TargetType="{x:Type l:SoftKey}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Background" Value="#10FFFFFF" />
        <Setter Property="BorderBrush" Value="#40FFFFFF" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="2" />
        <!--workaround for different charactor and different case have different width-->
        <Setter Property="MinWidth" Value="42" />
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="FontSize" Value="28" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type l:SoftKey}">
                    <Border x:Name="Border" Padding="{TemplateBinding Padding}" CornerRadius="0" Background="{TemplateBinding Background}" BorderThickness="2" BorderBrush="{TemplateBinding BorderBrush}">
                        <ContentPresenter x:Name="ContentSite" 
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <!--<Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#B0FFFFFF" />
                        </Trigger>-->
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="#10FFFFFF" />
                            <Setter TargetName="Border" Property="BorderBrush" Value="#40FFFFFF" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="#40FFFFFF" />
                        </Trigger>
                        <Trigger Property="IsToggleKey" Value="True">
                            <Setter TargetName="Border" Property="CornerRadius" Value="7"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsToggleKey" Value="True"/>
                                <Condition Property="IsChecked" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter  Property="Background" Value="#93c1db"/>
                        </MultiTrigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="#B0FFFFFF" />
                            <Setter Property="Background" Value="Orange" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--=================================================================
        SoftKeyboardTextBox
    ==================================================================-->

    <!-- toggle button-->
    <Style x:Key="ExpandButtonStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="#40FFFFFF" />
        <Setter Property="Foreground" Value="#F0F0F0" />
        <Setter Property="Background" Value="{StaticResource PageGlassyBackground}"/>
        <Setter Property="Width" Value="42"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Border 
                            Background="{TemplateBinding Background}" 
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}" >
                        <DockPanel HorizontalAlignment="Center">
                            <Path x:Name="arrow"
                                  Margin="4,-2,0,6" 
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Center"
                                  Fill="{TemplateBinding Foreground}" 
                                  StrokeThickness="0"
                                  Data="M 10,8 L 10,24 L 2,16 "/>
                            <TextBlock Margin="4,-1,4,3"
                                       FontFamily="Arial" 
                                       FontSize="30"
                                       FontWeight="Bold"
                                       VerticalAlignment="Center"
                                       Text="a"
                                       />
                            <!-- <TextBlock Text="3" Margin="1,1,0,0"
                                       FontFamily="Webdings" 
                                       FontSize="27"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       />
                            <TextBlock Margin="-9,-1,7,3" 
                                       FontFamily="Arial" 
                                       FontSize="30"
                                       FontWeight="Bold"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="a"
                                       /> -->
                        </DockPanel>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Background" Value="#93c1db"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="BorderBrush" Value="#B0FFFFFF" />
                            <Setter Property="Background" Value="Transparent" />
                            <Setter Property="BorderThickness" Value="2"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="#10FFFFFF" />
                            <Setter Property="BorderBrush" Value="#40FFFFFF" />
                            <Setter Property="BorderThickness" Value="2"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="#40FFFFFF" />
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter Property="Background" Value="Orange" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--TextBox-->
    <Style x:Key="{x:Type l:SoftKeyboardTextBox}" TargetType="{x:Type l:SoftKeyboardTextBox}">
        <Setter Property="BorderThickness" Value="4"/>
        <Setter Property="BorderBrush" Value="#40FFFFFF"/>
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="FontSize" Value="28" />
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="None"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="AllowDrop" Value="true"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type l:SoftKeyboardTextBox}">
                    <Grid>
                        <DockPanel>
                            <ToggleButton x:Name="PART_ExpandCollapseButton"
                                          DockPanel.Dock="Right" 
                                          Margin="4,0,0,0" 
                                          Padding="10,2,10,2"
                                          Style="{StaticResource ExpandButtonStyle}"
                                          IsChecked="{Binding Path=IsSoftKeyboardOpened,Mode=TwoWay,RelativeSource={RelativeSource TemplatedParent}}"/>
                            <Border Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}">
                                <ScrollViewer x:Name="PART_ContentHost"
                                              Margin="{TemplateBinding Padding}"/>
                            </Border>
                        </DockPanel>
                        <Popup Name="PART_Popup"
                               FocusManager.IsFocusScope="True" 
                               Placement="Bottom"
                               Focusable="False"
                               VerticalOffset="0" 
                               IsOpen="{Binding Path=IsSoftKeyboardOpened,RelativeSource={RelativeSource TemplatedParent}}"
                               PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}"
                               >
                            <Border Background="{StaticResource PageGlassyBackground}"
                                    BorderBrush="{StaticResource PageGlassyBackground}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    >
                                <l:SoftKeyboard x:Name="PART_SoftKeyboard" 
                                                SoftKeyContainerStyle="{TemplateBinding SoftKeyContainerStyle}"
                                                SoftKeyTemplate="{TemplateBinding SoftKeyTemplate}"
                                                SoftKeyTemplateSelector="{TemplateBinding SoftKeyTemplateSelector}"
                                                TargetTextBox="{Binding Mode=OneWay,RelativeSource={RelativeSource TemplatedParent}}"
                                                KeyboardNavigation.DirectionalNavigation="Cycle"
                                                />
                            </Border>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsFocused" Value="True"/>
                                <Condition Property="IsSoftKeyboardOpened" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="BorderBrush" Value="Orange"/>
                        </MultiTrigger>
                        <Trigger Property="Width" Value="Auto">
                            <Setter Property="MinWidth" Value="280" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

